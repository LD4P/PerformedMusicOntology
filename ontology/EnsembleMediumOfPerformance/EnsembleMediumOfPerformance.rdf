   xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
   xmlns:skos="http://www.w3.org/2004/02/skos/core#"
>
  <rdf:Description rdf:about="http://performedmusicontology.org/ontology/EnsembleMediumOfPerformance">
    <skos:definition>Named group of like or unlike mediums of performance intended to be played by multiple performers.</skos:definition>
    <rdfs:subClassOf rdf:resource="http://performedmusicontology.org/ontology/MediumOfPerformance"/>
    <rdfs:comment>Examples: orchestra, jazz combo, string quartet, percussion ensemble</rdfs:comment>
    <rdfs:label>Ensemble type</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
</rdf:RDF>
