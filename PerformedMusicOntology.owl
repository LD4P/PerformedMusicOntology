    <!-- http://id.loc.gov/ontologies/bibframe/eventContent -->

    <owl:ObjectProperty rdf:about="http://id.loc.gov/ontologies/bibframe/eventContent"/>
    


    <!-- http://id.loc.gov/ontologies/bibframe/eventContentOf -->

    <owl:ObjectProperty rdf:about="http://id.loc.gov/ontologies/bibframe/eventContentOf"/>
    


    <!-- http://id.loc.gov/ontologies/bibframe/relatedTo -->

    <owl:ObjectProperty rdf:about="http://id.loc.gov/ontologies/bibframe/relatedTo"/>
    


    <!-- http://performedmusicontology.org/ontology/alternateOf -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/alternateOf">
        <skos:definition>Declared medium part that is an alternate for another declared medium part</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/associatedWith -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/associatedWith">
        <rdfs:subPropertyOf rdf:resource="http://id.loc.gov/ontologies/bibframe/relatedTo"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:range rdf:resource="http://id.loc.gov/ontologies/bibframe/Event"/>
        <skos:definition>Event a work, instance, or item is created for, e.g., a motet created for the inauguration of Pope Clement VI.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/createdFor -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/createdFor">
        <rdfs:subPropertyOf rdf:resource="http://id.loc.gov/ontologies/bibframe/relatedTo"/>
        <rdfs:domain rdf:resource="http://id.loc.gov/ontologies/bibframe/Work"/>
        <rdfs:range rdf:resource="http://id.loc.gov/ontologies/bibframe/Event"/>
        <skos:definition>Event a work is created for, e.g., a motet celebrating the inauguration of a pope.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/eventNameOf -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/eventNameOf">
        <owl:inverseOf rdf:resource="http://performedmusicontology.org/ontology/hasEventName"/>
        <rdfs:range rdf:resource="http://id.loc.gov/ontologies/bibframe/Event"/>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasDoublingMediumofPerformance -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/hasDoublingMediumofPerformance">
        <skos:definition>2 (or more) mediums of performance intended to be performed alternately by a single performer</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasDramaticRole -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/hasDramaticRole"/>
    


    <!-- http://performedmusicontology.org/ontology/hasEventName -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/hasEventName">
        <rdfs:domain rdf:resource="http://id.loc.gov/ontologies/bibframe/Event"/>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasInspiration -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/hasInspiration">
        <rdfs:subPropertyOf rdf:resource="http://id.loc.gov/ontologies/bibframe/relatedTo"/>
        <owl:inverseOf rdf:resource="http://performedmusicontology.org/ontology/inspirationFor"/>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasMedium -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/hasMedium">
        <skos:definition>The declared or performed medium as a whole.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasMediumOfPerformance -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/hasMediumOfPerformance">
        <skos:definition>Specific medium of performance, e.g., bass guitar.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasMediumPart -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/hasMediumPart">
        <rdfs:comment>Use with: pmo:DeclaredMedium, pmo:PerformedMedium, pmo:AlternateMedium
bf:Contribution</rdfs:comment>
        <skos:definition>Specific element within an overall declared or performed medium</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasMediumPartType -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/hasMediumPartType">
        <skos:definition>Specific role or type of a medium part, e.g., solo, ad lib, optional, offstage.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasMusicPart -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/hasMusicPart"/>
    


    <!-- http://performedmusicontology.org/ontology/hasPerformance -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/hasPerformance">
        <rdfs:subPropertyOf rdf:resource="http://id.loc.gov/ontologies/bibframe/eventContentOf"/>
        <owl:inverseOf rdf:resource="http://performedmusicontology.org/ontology/performanceOf"/>
        <rdfs:domain rdf:resource="http://id.loc.gov/ontologies/bibframe/Work"/>
        <rdfs:range rdf:resource="http://performedmusicontology.org/ontology/Performance"/>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasProxyWork -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/hasProxyWork">
        <rdfs:comment>Work that is a duplicate of another work, except that it is included as part of an aggregation/compilation or a work component in multiples works. The proxy work permits numbering within the aggregate or work.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasRecording -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/hasRecording">
        <rdfs:subPropertyOf rdf:resource="http://id.loc.gov/ontologies/bibframe/eventContent"/>
        <owl:inverseOf rdf:resource="http://performedmusicontology.org/ontology/recordingOf"/>
        <rdfs:domain rdf:resource="http://id.loc.gov/ontologies/bibframe/Event"/>
        <rdfs:range rdf:resource="http://id.loc.gov/ontologies/bibframe/Work"/>
        <skos:definition>Audio or video recording of an event.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/inspirationFor -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/inspirationFor">
        <rdfs:subPropertyOf rdf:resource="http://id.loc.gov/ontologies/bibframe/relatedTo"/>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/performanceOf -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/performanceOf">
        <rdfs:subPropertyOf rdf:resource="http://id.loc.gov/ontologies/bibframe/eventContent"/>
        <rdfs:domain rdf:resource="http://id.loc.gov/ontologies/bibframe/Event"/>
        <rdfs:range rdf:resource="http://performedmusicontology.org/ontology/Performance"/>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/playsDramaticRole -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/playsDramaticRole"/>
    


    <!-- http://performedmusicontology.org/ontology/recordingOf -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/recordingOf">
        <rdfs:subPropertyOf rdf:resource="http://id.loc.gov/ontologies/bibframe/eventContentOf"/>
        <rdfs:domain rdf:resource="http://id.loc.gov/ontologies/bibframe/Work"/>
        <rdfs:range rdf:resource="http://id.loc.gov/ontologies/bibframe/Event"/>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/2004/02/skos/core#hasTopConcept -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/2004/02/skos/core#hasTopConcept"/>
    


