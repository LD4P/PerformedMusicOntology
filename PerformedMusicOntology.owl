        <skos:broader rdf:resource="http://rdaregistry.info/termList/RDACarrierType/1007"/>
        <skos:definition>Sony&apos;s proprietary Digital Audio Tape format, characterized by magnetic media enclosed in a protective shell, with the dimensions of 8 x 6 cm, 4 mm tape or 2 7/8 x 2 1/8 in., 1/8 in. tape</skos:definition>
    </owl:NamedIndividual>
    


    <!-- http://performedmusicontology.org/ontology/DvdAudio -->

    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/ontology/DvdAudio">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/Carrier"/>
