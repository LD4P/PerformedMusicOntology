        <rdfs:label>15 ips</rdfs:label>
        <skos:definition>Playing speed of 15 inches per second.</skos:definition>
    </owl:NamedIndividual>
    


    <!-- http://performedmusicontology.org/ontology/IPS3.75 -->

    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/ontology/IPS3.75">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/PlayingSpeed"/>
