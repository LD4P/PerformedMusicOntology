    </owl:NamedIndividual>
    


    <!-- http://performedmusicontology.org/Laserdisc -->

    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/Laserdisc">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/Carrier"/>
        <rdfs:label>Laser disc</rdfs:label>
        <skos:broader rdf:resource="http://rdaregistry.info/termList/RDACarrierType/1060"/>
        <skos:definition>Optical video disc, 30 cm (12 in.) in diameter. It was the first commercial optical disc storage medium.</skos:definition>
    </owl:NamedIndividual>
    


    <!-- http://performedmusicontology.org/adLib -->

    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/adLib">
        <rdf:type rdf:resource="http://performedmusicontology.org/ontology/MediumPartType"/>
        <rdfs:comment>For mediums of performance marked &quot;ad lib&quot; or &quot;ad libitum&quot; in a score or otherwise.</rdfs:comment>
        <rdfs:label>ad lib</rdfs:label>
        <skos:altLabel>ad libitum</skos:altLabel>
    </owl:NamedIndividual>
    


    <!-- http://performedmusicontology.org/offstage -->

    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/offstage">
        <rdf:type rdf:resource="http://performedmusicontology.org/ontology/MediumPartType"/>
        <rdfs:label>offstage</rdfs:label>
        <skos:definition>A declared or performed medium that is to be performed or is performed offstage, i.e., out of sight of the audience.</skos:definition>
    </owl:NamedIndividual>
    


    <!-- http://performedmusicontology.org/optional -->

    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/optional">
        <rdf:type rdf:resource="http://performedmusicontology.org/ontology/MediumPartType"/>
        <rdfs:label>optional</rdfs:label>
        <skos:definition>For mediums of performance marked &quot;optional&quot; in a score or otherwise.</skos:definition>
    </owl:NamedIndividual>
    


    <!-- http://performedmusicontology.org/solo -->

    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/solo">
        <rdf:type rdf:resource="http://performedmusicontology.org/ontology/MediumPartType"/>
        <rdfs:label>solo</rdfs:label>
        <skos:definition>For mediums of performance marked &quot;solo&quot; in a score or otherwise.</skos:definition>
    </owl:NamedIndividual>
    


    <!-- http://performedmusicontology.org/45 -->

    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/45">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/Carrier"/>
        <rdfs:label>45 recording</rdfs:label>
        <skos:definition>Colloquial term representing 7&quot; diameter vinyl audio discs with a playing speed of 45 rpm. The term does not necessarily accurately describe the carrier.</skos:definition>
    </owl:NamedIndividual>
    


    <!-- http://performedmusicontology.org/78recording -->

    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/78recording">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/Carrier"/>
        <rdfs:label>78 recording</rdfs:label>
        <skos:definition>Colloquial term representing audio discs from the 78 rpm disc era, with actual playing speeds ranging from around 60 rpm to 130 rpm.The term does not necessarily accurately describe the carrier.</skos:definition>
    </owl:NamedIndividual>
    


    <!-- http://performedmusicontology.org/ontology/AAC -->

    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/ontology/AAC">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/EncodingFormat"/>
        <skos:definition>Advanced Audio Coding format, a proprietary audio coding standard for lossy digital audio compression.</skos:definition>
    </owl:NamedIndividual>
    


    <!-- http://performedmusicontology.org/ontology/AIFF -->

    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/ontology/AIFF">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/EncodingFormat"/>
        <skos:definition>Standard audio file format used by Apple.</skos:definition>
    </owl:NamedIndividual>
    


    <!-- http://performedmusicontology.org/ontology/CdAudio -->

    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/ontology/CdAudio">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/Carrier"/>
        <rdfs:label>CD Audio</rdfs:label>
        <skos:broader rdf:resource="http://rdaregistry.info/termList/RDACarrierType/1004"/>
        <skos:definition>Optical discs that use the standard compact disc format for audio. They are 12 cm (4 3/4 in.) in diameter, and encoded as 2-channel signed 16-bit Linear PCM sampled at 44,100 Hz.</skos:definition>
    </owl:NamedIndividual>
    


    <!-- http://performedmusicontology.org/ontology/DAT -->

    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/ontology/DAT">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/Carrier"/>
