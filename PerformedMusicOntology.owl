    </owl:NamedIndividual>
    


    <!-- http://performedmusicontology.org/ontology/RPM8 -->

    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/ontology/RPM8">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/PlayingSpeed"/>
        <rdfs:comment>For audio discs</rdfs:comment>
        <rdfs:label>8 rpm</rdfs:label>
        <skos:definition>Playing speed of 8 revolutions per minute.</skos:definition>
