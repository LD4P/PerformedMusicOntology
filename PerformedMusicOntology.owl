    <!-- http://performedmusicontology.org/ontology/Audition -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/Audition">
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Event"/>
        <skos:definition>Event in which a performer or actor is applying for a position in an ensemble, cast, film, etc.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/BenefitConcert -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/BenefitConcert">
        <rdfs:subClassOf rdf:resource="http://performedmusicontology.org/ontology/Concert"/>
        <skos:definition>Concert held for charitable purposes, i.e., to raise funds or awareness</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/Ceremony -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/Ceremony">
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Event"/>
        <skos:definition>Ritual event, performed on special occasions (e.g., death, baptism, wedding, graduation, equinox, coronation, victory)</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/CommandPerformance -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/CommandPerformance">
        <rdfs:subClassOf rdf:resource="http://performedmusicontology.org/ontology/Concert"/>
        <skos:definition>Performance done at the request of a monarch or other head of state.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/Concert -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/Concert">
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Event"/>
        <skos:definition>Live musical performance of one or more works in front of an audience</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/ConcertSeries -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/ConcertSeries">
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Event"/>
        <skos:definition>Group of concerts in the same geographic area by a single performer or brought together by a common theme, usually available for sale as a package. e.g., an orchestra concert season, a series featuring chamber ensembles.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/ConcertTour -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/ConcertTour">
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Event"/>
        <skos:definition>Series of concerts by a performer or group of performers in different cities, countries, or locations.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/DeclaredMedium -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/DeclaredMedium">
        <skos:definition>Intended overall medium of performance of a musical work, as declared by the composer, a reference source, or an instance of the work.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/DiscCutting -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/DiscCutting">
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/SoundCharacteristic"/>
        <skos:definition>Recording process in which a stylus cuts a vertical or lateral groove into an audio disc.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/DramaticRole -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/DramaticRole"/>
    


    <!-- http://performedmusicontology.org/ontology/EventName -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/EventName">
        <skos:definition>Name given to an event.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/FirstPerformance -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/FirstPerformance">
        <rdfs:subClassOf rdf:resource="http://performedmusicontology.org/ontology/Performance"/>
        <skos:definition>The world premiere performance of a work.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/IndividualInstrument -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/IndividualInstrument">
        <rdfs:subClassOf rdf:resource="http://performedmusicontology.org/ontology/MusicalInstrument"/>
        <skos:definition>Single musical instrument, e.g., bassoon, bass guitar, synthesizer or group of instruments, e.g., percussion, intended to be played by a single performer.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/IndividualVoice -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/IndividualVoice">
        <rdfs:subClassOf rdf:resource="http://performedmusicontology.org/ontology/Voice"/>
        <rdfs:comment>Single voice, i.e., soprano, male voice, alto.</rdfs:comment>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/InstrumentEnsemble -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/InstrumentEnsemble">
        <rdfs:subClassOf rdf:resource="http://performedmusicontology.org/ontology/MusicalInstrument"/>
        <skos:definition>Group of like or unlike musical instruments intended to be played by multiple performers.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/LivePerformance -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/LivePerformance">
        <rdfs:subClassOf rdf:resource="http://performedmusicontology.org/ontology/Performance"/>
        <skos:definition>Performance that is recorded at a concert.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/MasterClass -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/MasterClass">
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Event"/>
        <skos:definition>Event in which a recognized performer critiques the performances of one or more other performers.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/MediumOfPerformance -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/MediumOfPerformance">
        <skos:definition>Instruments, voices, ensembles, choruses, and other participants (actors, dancers, etc.) required or used for an event or work, including electronics, computers, installations, etc.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/MediumPart -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/MediumPart">
        <skos:definition>Medium of performance, type, etc., of a specific part of the declared or performed medium.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/MediumPartType -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/MediumPartType">
        <rdfs:label>Medium type</rdfs:label>
        <skos:definition>The type of pmo:PerformedMediumPart, e.g., solo, ad lib, optional</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/MusicalInstrument -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/MusicalInstrument">
        <rdfs:subClassOf rdf:resource="http://performedmusicontology.org/ontology/MediumOfPerformance"/>
        <skos:definition>Musical instrument(s) or object(s) used as a musical instrument (e.g., drinking glasses, leaves, typewriters).</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/OpenMicPerformance -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/OpenMicPerformance">
        <rdfs:subClassOf rdf:resource="http://performedmusicontology.org/ontology/Performance"/>
        <skos:definition>Live performance at a coffee house, night club, or other venue by amateur performers.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/Performance -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/Performance">
        <rdfs:subClassOf rdf:resource="http://erlangen-crm.org/efrbroo/F31_Performance"/>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Event"/>
        <skos:definition>Event in which a performer or group of performers present a work to an audience or in a recording studio.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/PerformedMedium -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/PerformedMedium">
        <skos:definition>The overall medium of performance used in the performance of a musical work. This may or may not be the same as the declared medium of performance.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/ProxyWork -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/ProxyWork">
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Work"/>
        <skos:definition>A work that is aggregated into a compilation or other type of aggregating work. Ordering of the work within the aggregating work is put here.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/RecordingSession -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/RecordingSession">
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Event"/>
        <skos:definition>Event in which one or more performances are recorded with the aim of producing a recording for issue.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/Rehearsal -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/Rehearsal">
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Event"/>
        <skos:definition>Event in which a performer or group of performers prepare for a performance in a concert.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/Tempo -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/Tempo">
        <rdfs:isDefinedBy>The speed or pace of a music work, whether declared (in a score) or performed.</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/VideogamePlatformIdentifier -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/VideogamePlatformIdentifier">
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Identifier"/>
        <skos:definition>Identifier assigned to a video game platform, e.g., CUSA 54321 (a PlayStation 4 number).</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/Voice -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/Voice">
        <rdfs:subClassOf rdf:resource="http://performedmusicontology.org/ontology/MediumOfPerformance"/>
        <skos:definition>Human voice as a medium of performance, whether singing, speaking, or making sounds</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/VoiceEnsemble -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/VoiceEnsemble">
        <rdfs:subClassOf rdf:resource="http://performedmusicontology.org/ontology/Voice"/>
        <rdfs:comment>Group of like or unlike voices, e.g., children’s chorus.</rdfs:comment>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/WorkComponent -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/WorkComponent">
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Work"/>
        <rdfs:isDefinedBy>A work that is an integral part of another work, even when separated from it, e.g., a movement of a symphony, a chapter of a novel.</rdfs:isDefinedBy>
    </owl:Class>
    


