        <skos:altLabel>DVD-A</skos:altLabel>
        <skos:broader rdf:resource="http://rdaregistry.info/termList/RDACarrierType/1004"/>
        <skos:definition>DVD optical discs that use the the DVD audio format. Different bit depth/sampling rate/channel combinations can be used on a single disc.</skos:definition>
    </owl:NamedIndividual>
    


    <!-- http://performedmusicontology.org/ontology/DvdVideo -->

    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/ontology/DvdVideo">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/Carrier"/>
        <rdfs:label>DVD Video</rdfs:label>
        <skos:broader rdf:resource="http://rdaregistry.info/termList/RDACarrierType/1060"/>
        <skos:definition>DVD optical discs that use the DVD format to store digital video. From 2003, it has been the dominant consumer video format.</skos:definition>
    </owl:NamedIndividual>
    


    <!-- http://performedmusicontology.org/ontology/EightTrack -->

