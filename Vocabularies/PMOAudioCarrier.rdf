<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:dcterms="http://purl.org/dc/terms/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:rdact="http://rdaregistry.info/termList/RDACarrierType/">
       
         
   <rdf:Description rdf:about="http://performedmusicontology.org/ontologies/vocabularies/audio_carrier/">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
      <rdfs:label xml:lang="en">PMO Audio Carrier Vocabulary</rdfs:label>
      <dcterms:title xml:lang="en">PMO Audio Carrier Vocabulary</dcterms:title>
      <skos:definition xml:lang="en">The PMO Music Tonal Center Vocabulary is a controlled vocabulary for specific and colloquial terms for carriers of audio recordings. </skos:definition>
   </rdf:Description>
   
   <rdf:Description rdf:about="http://performedmusicontology.org/ontologies/vocabularies/audio_carrier/cd_audio">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:prefLabel xml:lang="en">CD Audio</skos:prefLabel>
      <skos:broader rdf:resource="http://rdaregistry.info/termList/RDACarrierType/1004"/>
      <skos:definition xml:lang="en">Optical discs that use the standard compact disc format for audio. They are 12 cm (4 3/4 in.) in diameter, and encoded as 2-channel signed 16-bit Linear PCM sampled at 44,100 Hz.</skos:definition>
      <skos:inScheme rdf:resource="http://performedmusicontology.org/ontologies/vocabularies/audio_carrier/"/>
      <skos:changeNote rdf:parseType="Resource">
         <rdf:value>NEW</rdf:value>
         <dcterms:date>2018-07-01</dcterms:date>
      </skos:changeNote>
   </rdf:Description>
  
   <rdf:Description rdf:about="http://performedmusicontology.org/ontologies/vocabularies/audio_carrier/dat">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:prefLabel xml:lang="en">DAT</skos:prefLabel>
      <skos:broader rdf:resource="http://rdaregistry.info/termList/RDACarrierType/1007"/>
      <skos:definition xml:lang="en">Sony's proprietary Digital Audio Tape format, characterized by magnetic media enclosed in a protective shell, with the dimensions of 8 x 6 cm, 4 mm tape or 2 7/8 x 2 1/8 in., 1/8 in. tape.</skos:definition>
      <skos:inScheme rdf:resource="http://performedmusicontology.org/ontologies/vocabularies/audio_carrier/"/>
      <skos:changeNote rdf:parseType="Resource">
         <rdf:value>NEW</rdf:value>
         <dcterms:date>2018-07-01</dcterms:date>
      </skos:changeNote>
   </rdf:Description>  
   
   
   <rdf:Description rdf:about="http://performedmusicontology.org/ontologies/vocabularies/audio_carrier/disc_45">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:prefLabel xml:lang="en">45 recording</skos:prefLabel>
      <skos:broader rdf:resource="http://rdaregistry.info/termList/RDACarrierType/1004"/>
      <skos:definition xml:lang="en">Colloquial term representing 7" diameter vinyl audio discs with a playing speed of 45 rpm. The term does not necessarily describe the carrier accurately.</skos:definition>
      <skos:inScheme rdf:resource="http://performedmusicontology.org/ontologies/vocabularies/audio_carrier/"/>
      <skos:changeNote rdf:parseType="Resource">
         <rdf:value>NEW</rdf:value>
         <dcterms:date>2018-07-01</dcterms:date>
      </skos:changeNote>
   </rdf:Description>
   
   <rdf:Description rdf:about="http://performedmusicontology.org/ontologies/vocabularies/audio_carrier/disc_78">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:prefLabel xml:lang="en">78 recording</skos:prefLabel>
      <skos:broader rdf:resource="http://rdaregistry.info/termList/RDACarrierType/1004"/>
      <skos:definition xml:lang="en">Colloquial term representing audio discs from the 78 rpm disc era with playing speeds ranging from around 60 rpm to 130 rpm. The term does not necessarily describe the carrier accurately.</skos:definition>
      <skos:inScheme rdf:resource="http://performedmusicontology.org/ontologies/vocabularies/audio_carrier/"/>
      <skos:changeNote rdf:parseType="Resource">
         <rdf:value>NEW</rdf:value>
         <dcterms:date>2018-07-01</dcterms:date>
      </skos:changeNote>
   </rdf:Description>
   
   <rdf:Description rdf:about="http://performedmusicontology.org/ontologies/vocabularies/audio_carrier/dvd_audio">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:prefLabel xml:lang="en">DVD Audio</skos:prefLabel>
      <skos:broader rdf:resource="http://rdaregistry.info/termList/RDACarrierType/1004"/>
      <skos:definition xml:lang="en">DVD optical discs that use the the DVD audio format. Different bit depth/sampling rate/channel combinations can be used on a single disc.</skos:definition>
      <skos:inScheme rdf:resource="http://performedmusicontology.org/ontologies/vocabularies/audio_carrier/"/>
      <skos:changeNote rdf:parseType="Resource">
         <rdf:value>NEW</rdf:value>
         <dcterms:date>2018-07-01</dcterms:date>
      </skos:changeNote>
   </rdf:Description>
   
   <rdf:Description rdf:about="http://performedmusicontology.org/ontologies/vocabularies/audio_carrier/dvd_video">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:prefLabel xml:lang="en">DVD Video</skos:prefLabel>
      <skos:broader rdf:resource="http://rdaregistry.info/termList/RDACarrierType/1060"/>
      <skos:definition xml:lang="en">DVD optical discs that use the DVD format to store digital video. From 2003, it has been the dominant consumer video format.</skos:definition>
      <skos:inScheme rdf:resource="http://performedmusicontology.org/ontologies/vocabularies/audio_carrier/"/>
      <skos:changeNote rdf:parseType="Resource">
         <rdf:value>NEW</rdf:value>
         <dcterms:date>2018-07-01</dcterms:date>
      </skos:changeNote>
   </rdf:Description>
   
   <rdf:Description rdf:about="http://performedmusicontology.org/ontologies/vocabularies/audio_carrier/eight_track">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:prefLabel xml:lang="en">8-track</skos:prefLabel>
      <skos:altLabel xml:lang="en">eight-track</skos:altLabel>
      <skos:broader rdf:resource="http://rdaregistry.info/termList/RDACarrierType/1008"/>
      <skos:definition xml:lang="en">Magnetic tape divided into 8 audio tracks that run parallel with each other, on which sound and other electro-acoustic signals are captured.</skos:definition>
      <skos:inScheme rdf:resource="http://performedmusicontology.org/ontologies/vocabularies/audio_carrier/"/>
      <skos:changeNote rdf:parseType="Resource">
         <rdf:value>NEW</rdf:value>
         <dcterms:date>2018-07-01</dcterms:date>
      </skos:changeNote>
   </rdf:Description>
   
   <rdf:Description rdf:about="http://performedmusicontology.org/ontologies/vocabularies/audio_carrier/enhanced_cd_audio">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:prefLabel xml:lang="en">Enhanced CD Audio</skos:prefLabel>
      <skos:broader rdf:resource="http://rdaregistry.info/termList/RDACarrierType/1004"/>
      <skos:definition xml:lang="en"></skos:definition>
      <skos:inScheme rdf:resource="http://performedmusicontology.org/ontologies/vocabularies/audio_carrier/"/>
      <skos:changeNote rdf:parseType="Resource">
         <rdf:value>NEW</rdf:value>
         <dcterms:date>2018-07-01</dcterms:date>
      </skos:changeNote>
   </rdf:Description>
   
   <rdf:Description rdf:about="http://performedmusicontology.org/ontologies/vocabularies/audio_carrier/ep_record">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:prefLabel xml:lang="en">EP record</skos:prefLabel>
      <skos:altLabel xml:lang="en">extended play record</skos:altLabel>
      <skos:broader rdf:resource="http://rdaregistry.info/termList/RDACarrierType/1004"/>
      <skos:definition xml:lang="en">A music disc that contains more tracks than a single but not as many as on a typical album/LP/disc.</skos:definition>
      <skos:inScheme rdf:resource="http://performedmusicontology.org/ontologies/vocabularies/audio_carrier/"/>
      <skos:changeNote rdf:parseType="Resource">
         <rdf:value>NEW</rdf:value>
         <dcterms:date>2018-07-01</dcterms:date>
      </skos:changeNote>
   </rdf:Description>
   
   <rdf:Description rdf:about="http://performedmusicontology.org/ontologies/vocabularies/audio_carrier/full_track">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:prefLabel xml:lang="en">full track</skos:prefLabel>
      <skos:broader rdf:resource="http://rdaregistry.info/termList/RDACarrierType/1008"/>
      <skos:definition xml:lang="en">Magnetic tape with 1 audio track on which sound and other electro-acoustic signals are captured.</skos:definition>
      <skos:inScheme rdf:resource="http://performedmusicontology.org/ontologies/vocabularies/audio_carrier/"/>
      <skos:changeNote rdf:parseType="Resource">
         <rdf:value>NEW</rdf:value>
         <dcterms:date>2018-07-01</dcterms:date>
      </skos:changeNote>
   </rdf:Description>
   
   <rdf:Description rdf:about="http://performedmusicontology.org/ontologies/vocabularies/audio_carrier/half_track">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:prefLabel xml:lang="en">half track</skos:prefLabel>
      <skos:altLabel xml:lang="en">1/2 track</skos:altLabel>
      <skos:broader rdf:resource="http://rdaregistry.info/termList/RDACarrierType/1008"/>
      <skos:definition xml:lang="en">Magnetic tape divided into 2 audio tracks that run parallel with each other, on which sound and other electro-acoustic signals are captured.</skos:definition>
      <skos:inScheme rdf:resource="http://performedmusicontology.org/ontologies/vocabularies/audio_carrier/"/>
      <skos:changeNote rdf:parseType="Resource">
         <rdf:value>NEW</rdf:value>
         <dcterms:date>2018-07-01</dcterms:date>
      </skos:changeNote>
   </rdf:Description>
   
   <rdf:Description rdf:about="http://performedmusicontology.org/ontologies/vocabularies/audio_carrier/hybrid_sacd">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:prefLabel xml:lang="en">hybrid SACD</skos:prefLabel>
      <skos:broader rdf:resource="http://rdaregistry.info/termList/RDACarrierType/1004"/>
      <skos:definition xml:lang="en">An SACD that also is also encoded with a PCM audio layer readable by compact disc players.</skos:definition>
      <skos:inScheme rdf:resource="http://performedmusicontology.org/ontologies/vocabularies/audio_carrier/"/>
      <skos:changeNote rdf:parseType="Resource">
         <rdf:value>NEW</rdf:value>
         <dcterms:date>2018-07-01</dcterms:date>
      </skos:changeNote>
   </rdf:Description>
   
   <rdf:Description rdf:about="http://performedmusicontology.org/ontologies/vocabularies/audio_carrier/lp">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:prefLabel xml:lang="en">LP</skos:prefLabel>
      <skos:broader rdf:resource="http://rdaregistry.info/termList/RDACarrierType/1004"/>
      <skos:definition xml:lang="en">Colloquial term representing 12-inch diameter vinyl audio discs with a playing speed of 33 1/3 rpm. The term does not necessarily accurately describe the carrier.</skos:definition>
      <skos:inScheme rdf:resource="http://performedmusicontology.org/ontologies/vocabularies/audio_carrier/"/>
      <skos:changeNote rdf:parseType="Resource">
         <rdf:value>NEW</rdf:value>
         <dcterms:date>2018-07-01</dcterms:date>
      </skos:changeNote>
   </rdf:Description>
   
   <rdf:Description rdf:about="http://performedmusicontology.org/ontologies/vocabularies/audio_carrier/laser_disc">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:prefLabel xml:lang="en">laserdisc</skos:prefLabel>
      <skos:broader rdf:resource="http://rdaregistry.info/termList/RDACarrierType/1004"/>
      <skos:definition xml:lang="en">Home video format, with a standard diameter of 30 cm (12 in.)</skos:definition>
      <skos:inScheme rdf:resource="http://performedmusicontology.org/ontologies/vocabularies/audio_carrier/"/>
      <skos:changeNote rdf:parseType="Resource">
         <rdf:value>NEW</rdf:value>
         <dcterms:date>2018-07-01</dcterms:date>
      </skos:changeNote>
   </rdf:Description>
   
   <rdf:Description rdf:about="http://performedmusicontology.org/ontologies/vocabularies/audio_carrier/organ_roll">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:prefLabel xml:lang="en">organ roll</skos:prefLabel>
      <skos:broader rdf:resource="http://rdaregistry.info/termList/RDACarrierType/1006"/>
      <skos:definition xml:lang="en">An audio roll used to operate a player organ or reproducing organ.</skos:definition>
      <skos:inScheme rdf:resource="http://performedmusicontology.org/ontologies/vocabularies/audio_carrier/"/>
      <skos:changeNote rdf:parseType="Resource">
         <rdf:value>NEW</rdf:value>
         <dcterms:date>2018-07-01</dcterms:date>
      </skos:changeNote>
   </rdf:Description>
   
   <rdf:Description rdf:about="http://performedmusicontology.org/ontologies/vocabularies/audio_carrier/piano_roll">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:prefLabel xml:lang="en">piano roll</skos:prefLabel>
      <skos:broader rdf:resource="http://rdaregistry.info/termList/RDACarrierType/1006"/>
      <skos:definition xml:lang="en">An audio roll used to operate a player piano or reproducing piano.</skos:definition>
      <skos:inScheme rdf:resource="http://performedmusicontology.org/ontologies/vocabularies/audio_carrier/"/>
      <skos:changeNote rdf:parseType="Resource">
         <rdf:value>NEW</rdf:value>
         <dcterms:date>2018-07-01</dcterms:date>
      </skos:changeNote>
   </rdf:Description>
   
   <rdf:Description rdf:about="http://performedmusicontology.org/ontologies/vocabularies/audio_carrier/quarter_track">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:prefLabel xml:lang="en">quarter track</skos:prefLabel>
      <skos:altLabel xml:lang="en">1/4 track</skos:altLabel>
      <skos:broader rdf:resource="http://rdaregistry.info/termList/RDACarrierType/1008"/>
      <skos:definition xml:lang="en">Magnetic tape divided into 4 audio tracks that run parallel with each other, in which sound and other electro-acoustic signals are captured.</skos:definition>
      <skos:inScheme rdf:resource="http://performedmusicontology.org/ontologies/vocabularies/audio_carrier/"/>
      <skos:changeNote rdf:parseType="Resource">
         <rdf:value>NEW</rdf:value>
         <dcterms:date>2018-07-01</dcterms:date>
      </skos:changeNote>
   </rdf:Description>
   
   <rdf:Description rdf:about="http://performedmusicontology.org/ontologies/vocabularies/audio_carrier/sacd">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:prefLabel xml:lang="en">SACD</skos:prefLabel>
      <skos:altLabel xml:lang="en">Super Audio Compact Disc</skos:altLabel>
      <skos:broader rdf:resource="http://rdaregistry.info/termList/RDACarrierType/1004"/>
      <skos:definition xml:lang="en">Optical audio disc with more channels and longer playing time than a conventional CD.</skos:definition>
      <skos:inScheme rdf:resource="http://performedmusicontology.org/ontologies/vocabularies/audio_carrier/"/>
      <skos:changeNote rdf:parseType="Resource">
         <rdf:value>NEW</rdf:value>
         <dcterms:date>2018-07-01</dcterms:date>
      </skos:changeNote>
   </rdf:Description>
   
   <rdf:Description rdf:about="http://performedmusicontology.org/ontologies/vocabularies/audio_carrier/sixteen_track">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:prefLabel xml:lang="en">16-track</skos:prefLabel>
      <skos:altLabel xml:lang="en">sixteen-track</skos:altLabel>
      <skos:broader rdf:resource="http://rdaregistry.info/termList/RDACarrierType/1008"/>
      <skos:definition xml:lang="en">Magnetic tape divided into 16 audio tracks that run parallel with each other, in which sound and other electro-acoustic signals are captured.</skos:definition>
      <skos:inScheme rdf:resource="http://performedmusicontology.org/ontologies/vocabularies/audio_carrier/"/>
      <skos:changeNote rdf:parseType="Resource">
         <rdf:value>NEW</rdf:value>
         <dcterms:date>2018-07-01</dcterms:date>
      </skos:changeNote>
   </rdf:Description>
   
   <rdf:Description rdf:about="http://performedmusicontology.org/ontologies/vocabularies/audio_carrier/twelve_track">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:prefLabel xml:lang="en">12-track</skos:prefLabel>
      <skos:altLabel xml:lang="en">twelve-track</skos:altLabel>
      <skos:broader rdf:resource="http://rdaregistry.info/termList/RDACarrierType/1008"/>
      <skos:definition xml:lang="en">Magnetic tape divided into 12 audio tracks that run parallel with each other, in which sound and other electro-acoustic signals are captured.</skos:definition>
      <skos:inScheme rdf:resource="http://performedmusicontology.org/ontologies/vocabularies/audio_carrier/"/>
      <skos:changeNote rdf:parseType="Resource">
         <rdf:value>NEW</rdf:value>
