    <!-- http://erlangen-crm.org/efrbroo/F31_Performance -->

    <owl:Class rdf:about="http://erlangen-crm.org/efrbroo/F31_Performance"/>
    


    <!-- http://id.loc.gov/ontologies/bibframe/Carrier -->

    <owl:Class rdf:about="http://id.loc.gov/ontologies/bibframe/Carrier"/>
    


    <!-- http://id.loc.gov/ontologies/bibframe/Contribution -->

    <owl:Class rdf:about="http://id.loc.gov/ontologies/bibframe/Contribution"/>
    


    <!-- http://id.loc.gov/ontologies/bibframe/EncodingFormat -->

    <owl:Class rdf:about="http://id.loc.gov/ontologies/bibframe/EncodingFormat"/>
    


    <!-- http://id.loc.gov/ontologies/bibframe/Event -->

    <owl:Class rdf:about="http://id.loc.gov/ontologies/bibframe/Event"/>
    


    <!-- http://id.loc.gov/ontologies/bibframe/Identifier -->

    <owl:Class rdf:about="http://id.loc.gov/ontologies/bibframe/Identifier"/>
    


    <!-- http://id.loc.gov/ontologies/bibframe/Organization -->

    <owl:Class rdf:about="http://id.loc.gov/ontologies/bibframe/Organization"/>
    


    <!-- http://id.loc.gov/ontologies/bibframe/PlayingSpeed -->

    <owl:Class rdf:about="http://id.loc.gov/ontologies/bibframe/PlayingSpeed"/>
    


    <!-- http://id.loc.gov/ontologies/bibframe/SoundCharacteristic -->

    <owl:Class rdf:about="http://id.loc.gov/ontologies/bibframe/SoundCharacteristic"/>
    


    <!-- http://id.loc.gov/ontologies/bibframe/TapeConfig -->

    <owl:Class rdf:about="http://id.loc.gov/ontologies/bibframe/TapeConfig"/>
    


    <!-- http://id.loc.gov/ontologies/bibframe/Work -->

    <owl:Class rdf:about="http://id.loc.gov/ontologies/bibframe/Work"/>
    


    <!-- http://performedmusicontology.org/ontology/Audition -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/Audition">
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Event"/>
        <skos:definition>Event in which a performer or actor is applying for a position in an ensemble, cast, film, etc.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/BenefitConcert -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/BenefitConcert">
        <rdfs:subClassOf rdf:resource="http://performedmusicontology.org/ontology/Concert"/>
        <skos:definition>Concert held for charitable purposes, i.e., to raise funds or awareness</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/Ceremony -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/Ceremony">
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Event"/>
        <skos:definition>Ritual event, performed on special occasions (e.g., death, baptism, wedding, graduation, equinox, coronation, victory)</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/CommandPerformance -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/CommandPerformance">
        <rdfs:subClassOf rdf:resource="http://performedmusicontology.org/ontology/Concert"/>
        <skos:definition>Performance done at the request of a monarch or other head of state.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/Concert -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/Concert">
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Event"/>
        <skos:definition>Live musical performance of one or more works in front of an audience</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/ConcertSeries -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/ConcertSeries">
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Event"/>
        <skos:definition>Group of concerts in the same geographic area by a single performer or brought together by a common theme, usually available for sale as a package. e.g., an orchestra concert season, a series featuring chamber ensembles.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/ConcertTour -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/ConcertTour">
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Event"/>
        <skos:definition>Series of concerts by a performer or group of performers in different cities, countries, or locations.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/DeclaredMedium -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/DeclaredMedium">
        <rdfs:label>Declared medium</rdfs:label>
        <skos:definition>Intended overall medium of performance of a musical work, as declared by the composer, a reference source, or other source.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/DiscCutting -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/DiscCutting">
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/SoundCharacteristic"/>
        <rdfs:label>Disc cutting process</rdfs:label>
        <skos:definition>Recording process in which a stylus cuts a vertical or lateral groove into an audio disc.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/Discogs -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/Discogs">
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Identifier"/>
        <rdfs:label>Discogs ID</rdfs:label>
        <skos:definition>Identifier from Discogs.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/DramaticRole -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/DramaticRole">
        <rdfs:label>Dramatic role</rdfs:label>
        <skos:definition>Character of a performer in a dramatic work (e.g., an opera, play, video), e.g., &quot;Carmen&quot; in the opera or video &quot;Carmen&quot;.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/EnsembleMediumOfPerformance -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/EnsembleMediumOfPerformance">
        <rdfs:subClassOf rdf:resource="http://performedmusicontology.org/ontology/MediumOfPerformance"/>
        <skos:definition>Named group of like or unlike mediums of performance intended to be played by multiple performers, e.g., orchestra, jazz combo, string quartet, percussion ensemble.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/EventName -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/EventName">
        <rdfs:label>Event name</rdfs:label>
        <skos:definition>Name given to an event.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/FirstPerformance -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/FirstPerformance">
        <rdfs:subClassOf rdf:resource="http://performedmusicontology.org/ontology/Performance"/>
        <skos:definition>The world premiere performance of a work.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/IndividualMediumOfPerformance -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/IndividualMediumOfPerformance">
        <rdfs:subClassOf rdf:resource="http://performedmusicontology.org/ontology/MediumOfPerformance"/>
        <rdfs:comment>Individual musical instrument, voice, or other object used as a medium of performance (e.g., flute, keyboards, pi pa, drinking glass, typewriter).</rdfs:comment>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/KeyMode -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/KeyMode">
        <rdfs:label>Music key and mode</rdfs:label>
        <skos:definition>Key and mode of a music work when used in combination, e.g., C major, B-flat major.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/LivePerformance -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/LivePerformance">
        <rdfs:subClassOf rdf:resource="http://performedmusicontology.org/ontology/Performance"/>
        <skos:definition>Performance that is recorded at a concert.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/MasterClass -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/MasterClass">
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Event"/>
        <skos:definition>Event in which a recognized performer critiques the performances of one or more other performers.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/MediumOfPerformance -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/MediumOfPerformance">
        <rdfs:label>Medium of performance</rdfs:label>
        <skos:definition>Instruments, voices, ensembles, choruses, and other participants (actors, dancers, etc.) required for, or performing in, an event or work, including electronics, computers, installations, etc.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/MediumPart -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/MediumPart">
        <rdfs:comment>Use with DeclaredMedium, PerformedMedium</rdfs:comment>
        <rdfs:label>Medium part</rdfs:label>
        <skos:definition>Medium of performance, type, etc., of a specific part of the declared or performed medium.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/MediumPartType -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/MediumPartType">
        <rdfs:label>Part type</rdfs:label>
        <skos:definition>Specific role or type of a medium part, e.g., solo, ad lib, optional, offstage, amplified</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/Mode -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/Mode">
        <rdfs:label>Music mode</rdfs:label>
        <skos:definition>Musical mode of a music work, e.g., Dorian, Dhaivata.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/MusicBrainz -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/MusicBrainz">
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Identifier"/>
        <rdfs:label>MusicBrainz ID</rdfs:label>
        <skos:definition>Identifier from MusicBrainz</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/MusicPart -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/MusicPart">
        <rdfs:label>Music part</rdfs:label>
        <skos:definition>Specific element within an overall declared or performed medium, e.g., Violin 1, Trumpet 2.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/OpenMicPerformance -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/OpenMicPerformance">
        <rdfs:subClassOf rdf:resource="http://performedmusicontology.org/ontology/Performance"/>
        <skos:definition>Live performance at a coffee house, night club, or other venue by amateur performers.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/OpusNumber -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/OpusNumber">
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Identifier"/>
        <rdfs:label>Opus number</rdfs:label>
        <skos:definition>Opus number for a   musical work, e.g. “45” of Op. 45. No. 1</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/OpusNumberPart -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/OpusNumberPart">
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Identifier"/>
        <rdfs:label>Opus number part</rdfs:label>
        <skos:definition>Number for a   portion of an opus number, e.g., no. 1   of Op. 45, no. 1.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/OpusNumberStatement -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/OpusNumberStatement">
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Identifier"/>
        <rdfs:label>Opus number statement</rdfs:label>
        <skos:definition>Statement about the opus number for a musical work.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/Performance -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/Performance">
        <rdfs:subClassOf rdf:resource="http://erlangen-crm.org/efrbroo/F31_Performance"/>
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Event"/>
        <skos:definition>Event in which a performer or group of performers present a work to an audience or in a recording studio.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/PerformedMedium -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/PerformedMedium">
        <rdfs:label>Performed medium</rdfs:label>
        <skos:definition>The overall medium of performance used in the performance of a musical work. This may or may not be the same as the declared medium of performance.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/RecordingSession -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/RecordingSession">
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Event"/>
        <skos:definition>Event in which one or more performances are recorded with the aim of producing a recording for issue.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/Rehearsal -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/Rehearsal">
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Event"/>
        <skos:definition>Event in which a performer or group of performers prepare for a performance in a concert.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/RismNumber -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/RismNumber">
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Identifier"/>
        <rdfs:label>RISM number</rdfs:label>
        <skos:definition>Number from the Repertoire Internationale des Sources Musicales (RISM) online version.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/Tempo -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/Tempo">
        <rdfs:label>Tempo</rdfs:label>
        <skos:definition>The speed or pace of a music work, whether declared (in a score) or performed.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/ThematicCatalogNumber -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/ThematicCatalogNumber">
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Identifier"/>
        <rdfs:label>Thematic catalog number</rdfs:label>
        <skos:definition>Numeric or alphanumericdesignation of a   musical work as found in a thematic index for the composer.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/ThematicCatalogPrefix -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/ThematicCatalogPrefix">
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Identifier"/>
        <rdfs:label>Thematic catalog prefix</rdfs:label>
        <skos:definition>Prefix assigned to a   thematic catalog number denoting thework from which the number originated, e.g., BWV for Bach, M.S. for Paganini.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/ThematicCatalogStatement -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/ThematicCatalogStatement">
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Identifier"/>
        <rdfs:label>Thematic catalog number statement</rdfs:label>
        <skos:definition>Statement about the thematic catalog number for a musical work.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/TonalCenter -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/TonalCenter">
        <rdfs:label>Tonal center</rdfs:label>
        <skos:definition>The tonal center of a music work, without mode, e.g., D, C##.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/VideogamePlatformIdentifier -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/VideogamePlatformIdentifier">
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Identifier"/>
        <rdfs:label>Videogame platform identifier</rdfs:label>
        <skos:definition>Identifier assigned to a video game platform, e.g., CUSA 54321 (a PlayStation 4 number).</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/WorkComponent -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/WorkComponent">
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Work"/>
        <rdfs:label>Work component</rdfs:label>
        <skos:definition>A work that is an integral part of another work, even when separated from it, e.g., a movement of a symphony, a chapter of a novel.</skos:definition>
    </owl:Class>
    


    <!-- http://www.w3.org/2004/02/skos/core#Concept -->

    <owl:Class rdf:about="http://www.w3.org/2004/02/skos/core#Concept"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#ConceptScheme -->

    <owl:Class rdf:about="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://performedmusicontology.org/FPM12 -->

    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/FPM12">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/PlayingSpeed"/>
        <rdfs:comment>Traditional way of describing the speed of audio cylinders.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">120</rdfs:label>
        <skos:definition>Playing speed of 12 feet per minute x 10.</skos:definition>
    </owl:NamedIndividual>
    


    <!-- http://performedmusicontology.org/Laserdisc -->

    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/Laserdisc">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/Carrier"/>
        <rdfs:label>Laser disc</rdfs:label>
        <skos:broader rdf:resource="http://rdaregistry.info/termList/RDACarrierType/1060"/>
        <skos:definition>Optical video disc, 30 cm (12 in.) in diameter. It was the first commercial optical disc storage medium.</skos:definition>
    </owl:NamedIndividual>
    


    <!-- http://performedmusicontology.org/adLib -->

    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/adLib">
        <rdf:type rdf:resource="http://performedmusicontology.org/ontology/MediumPartType"/>
        <rdfs:comment>For mediums of performance marked &quot;ad lib&quot; or &quot;ad libitum&quot; in a score or otherwise.</rdfs:comment>
        <rdfs:label>ad lib</rdfs:label>
        <skos:altLabel>ad libitum</skos:altLabel>
    </owl:NamedIndividual>
    


    <!-- http://performedmusicontology.org/offstage -->

    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/offstage">
        <rdf:type rdf:resource="http://performedmusicontology.org/ontology/MediumPartType"/>
        <rdfs:label>offstage</rdfs:label>
        <skos:definition>A declared or performed medium that is to be performed or is performed offstage, i.e., out of sight of the audience.</skos:definition>
    </owl:NamedIndividual>
    


    <!-- http://performedmusicontology.org/optional -->

    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/optional">
        <rdf:type rdf:resource="http://performedmusicontology.org/ontology/MediumPartType"/>
        <rdfs:label>optional</rdfs:label>
        <skos:definition>For mediums of performance marked &quot;optional&quot; in a score or otherwise.</skos:definition>
    </owl:NamedIndividual>
    


    <!-- http://performedmusicontology.org/solo -->

    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/solo">
        <rdf:type rdf:resource="http://performedmusicontology.org/ontology/MediumPartType"/>
        <rdfs:label>solo</rdfs:label>
        <skos:definition>For mediums of performance marked &quot;solo&quot; in a score or is clearly a solo part in the context of the work, e.g., a concerto for oboe and orchestra.</skos:definition>
    </owl:NamedIndividual>
    


    <!-- http://performedmusicontology.org/45 -->

    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/45">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/Carrier"/>
        <rdfs:label>45 recording</rdfs:label>
        <skos:broader rdf:resource="http://rdaregistry.info/termList/RDACarrierType/1004"/>
        <skos:definition>Colloquial term representing 7&quot; diameter vinyl audio discs with a playing speed of 45 rpm. The term does not necessarily accurately describe the carrier.</skos:definition>
    </owl:NamedIndividual>
    


    <!-- http://performedmusicontology.org/78recording -->

    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/78recording">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/Carrier"/>
        <rdfs:label>78 recording</rdfs:label>
        <skos:broader rdf:resource="http://rdaregistry.info/termList/RDACarrierType/1004"/>
        <skos:definition>Colloquial term representing audio discs from the 78 rpm disc era, with actual playing speeds ranging from around 60 rpm to 130 rpm.The term does not necessarily accurately describe the carrier.</skos:definition>
    </owl:NamedIndividual>
    


    <!-- http://performedmusicontology.org/ontology/AAC -->

    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/ontology/AAC">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/EncodingFormat"/>
        <rdfs:label>AAC</rdfs:label>
        <skos:definition>Advanced Audio Coding format, a proprietary audio coding standard for lossy digital audio compression.</skos:definition>
    </owl:NamedIndividual>
    


    <!-- http://performedmusicontology.org/ontology/AIFF -->

    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/ontology/AIFF">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/EncodingFormat"/>
        <rdfs:label>AIFF</rdfs:label>
        <skos:definition>Standard audio file format used by Apple.</skos:definition>
    </owl:NamedIndividual>
    


    <!-- http://performedmusicontology.org/ontology/CdAudio -->

    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/ontology/CdAudio">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/Carrier"/>
        <rdfs:label>CD Audio</rdfs:label>
        <skos:broader rdf:resource="http://rdaregistry.info/termList/RDACarrierType/1004"/>
        <skos:definition>Optical discs that use the standard compact disc format for audio. They are 12 cm (4 3/4 in.) in diameter, and encoded as 2-channel signed 16-bit Linear PCM sampled at 44,100 Hz.</skos:definition>
    </owl:NamedIndividual>
    


    <!-- http://performedmusicontology.org/ontology/DAT -->

    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/ontology/DAT">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/Carrier"/>
        <rdfs:label>DAT (Digital Audio Tape)</rdfs:label>
        <skos:broader rdf:resource="http://rdaregistry.info/termList/RDACarrierType/1007"/>
        <skos:definition>Sony&apos;s proprietary Digital Audio Tape format, characterized by magnetic media enclosed in a protective shell, with the dimensions of 8 x 6 cm, 4 mm tape or 2 7/8 x 2 1/8 in., 1/8 in. tape</skos:definition>
    </owl:NamedIndividual>
    


    <!-- http://performedmusicontology.org/ontology/DvdAudio -->

    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/ontology/DvdAudio">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/Carrier"/>
        <rdfs:label>DVD Audio</rdfs:label>
        <skos:altLabel>DVD-A</skos:altLabel>
        <skos:broader rdf:resource="http://rdaregistry.info/termList/RDACarrierType/1004"/>
        <skos:definition>DVD optical discs that use the the DVD audio format. Different bit depth/sampling rate/channel combinations can be used on a single disc.</skos:definition>
    </owl:NamedIndividual>
    


    <!-- http://performedmusicontology.org/ontology/DvdVideo -->

    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/ontology/DvdVideo">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/Carrier"/>
        <rdfs:label>DVD Video</rdfs:label>
        <skos:broader rdf:resource="http://rdaregistry.info/termList/RDACarrierType/1060"/>
        <skos:definition>DVD optical discs that use the DVD format to store digital video. From 2003, it has been the dominant consumer video format.</skos:definition>
    </owl:NamedIndividual>
    


    <!-- http://performedmusicontology.org/ontology/EightTrack -->

    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/ontology/EightTrack">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/TapeConfig"/>
        <rdfs:label>8-track</rdfs:label>
        <skos:definition>Magnetic tape divided into 8 audio tracks that run parallel with each other, on which sound and other electro-acoustic signals are captured.</skos:definition>
    </owl:NamedIndividual>
    


    <!-- http://performedmusicontology.org/ontology/EnhancedCdAudio -->

    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/ontology/EnhancedCdAudio">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/Carrier"/>
        <rdfs:label>Enhanced CD Audio</rdfs:label>
        <skos:broader rdf:resource="http://rdaregistry.info/termList/RDACarrierType/1004"/>
        <skos:definition>An optical disc containing a combination of audio and computer data for use in both CD and CD-ROM players. Usually music CDs combined with short videos or other content.</skos:definition>
    </owl:NamedIndividual>
    


    <!-- http://performedmusicontology.org/ontology/EpRecord -->

    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/ontology/EpRecord">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/Carrier"/>
        <rdfs:label>EP (Extended play)</rdfs:label>
        <skos:broader rdf:resource="http://rdaregistry.info/termList/RDACarrierType/1004"/>
        <skos:definition>A music disc that contains more tracks than a single but not as many as on a typical album/LP/disc.</skos:definition>
    </owl:NamedIndividual>
    


    <!-- http://performedmusicontology.org/ontology/FLAC -->

    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/ontology/FLAC">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/EncodingFormat"/>
        <rdfs:label>FLAC</rdfs:label>
        <skos:definition>Free Lossless Audio Codec, an open audio coding format for lossless compression of digital audio.</skos:definition>
    </owl:NamedIndividual>
    


    <!-- http://performedmusicontology.org/ontology/FullTrack -->

    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/ontology/FullTrack">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/TapeConfig"/>
        <rdfs:label>full track</rdfs:label>
        <skos:definition>Magnetic tape with 1 audio track on which sound and other electro-acoustic signals are captured.</skos:definition>
    </owl:NamedIndividual>
    


    <!-- http://performedmusicontology.org/ontology/HalfTrack -->

    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/ontology/HalfTrack">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/TapeConfig"/>
        <rdfs:label>half track</rdfs:label>
        <skos:definition>Magnetic tape divided into 2 audio tracks that run parallel with each other, on which sound and other electro-acoustic signals are captured.</skos:definition>
    </owl:NamedIndividual>
    


    <!-- http://performedmusicontology.org/ontology/HybridSacd -->

    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/ontology/HybridSacd">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/Carrier"/>
        <rdfs:label>Hybrid SACD</rdfs:label>
        <skos:broader rdf:resource="http://rdaregistry.info/termList/RDACarrierType/1004"/>
        <skos:definition>An SACD that also is also encoded with a PCM audio layer readable by compact disc players.</skos:definition>
    </owl:NamedIndividual>
    


    <!-- http://performedmusicontology.org/ontology/IPS.9 -->

    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/ontology/IPS.9">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/PlayingSpeed"/>
        <rdfs:comment>Speed occurs on half-speed mini-cassettes used in small portable recorders.</rdfs:comment>
        <rdfs:label>15/16 ips</rdfs:label>
        <skos:definition>Playing speed of 15/16 inches per second.</skos:definition>
    </owl:NamedIndividual>
    


    <!-- http://performedmusicontology.org/ontology/IPS1.8 -->

    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/ontology/IPS1.8">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/PlayingSpeed"/>
        <rdfs:comment>Speed occurs on standard audiocassettes.</rdfs:comment>
        <rdfs:label>1 7/8 ips</rdfs:label>
        <skos:definition>Playing speed of 1 7/8 inches per second.</skos:definition>
    </owl:NamedIndividual>
    


    <!-- http://performedmusicontology.org/ontology/IPS15 -->

    <owl:NamedIndividual rdf:about="http://performedmusicontology.org/ontology/IPS15">
        <rdf:type rdf:resource="http://id.loc.gov/ontologies/bibframe/PlayingSpeed"/>
        <rdfs:comment>Occcurs on reel-to-reel, studio tapes.</rdfs:comment>
        <rdfs:label>15 ips</rdfs:label>
        <skos:definition>Playing speed of 15 inches per second.</skos:definition>
