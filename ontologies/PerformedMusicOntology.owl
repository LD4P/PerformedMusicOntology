    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#scopeNote"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://id.loc.gov/ontologies/bibframe/eventContent -->

    <owl:ObjectProperty rdf:about="http://id.loc.gov/ontologies/bibframe/eventContent"/>
    


    <!-- http://id.loc.gov/ontologies/bibframe/eventContentOf -->

    <owl:ObjectProperty rdf:about="http://id.loc.gov/ontologies/bibframe/eventContentOf"/>
    


    <!-- http://id.loc.gov/ontologies/bibframe/relatedTo -->

    <owl:ObjectProperty rdf:about="http://id.loc.gov/ontologies/bibframe/relatedTo"/>
    


    <!-- http://performedmusicontology.org/ontology/asMemberOf -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/asMemberOf">
        <rdfs:domain rdf:resource="http://id.loc.gov/ontologies/bibframe/Contribution"/>
        <rdfs:range rdf:resource="http://id.loc.gov/ontologies/bibframe/Organization"/>
        <rdfs:label>as Member of</rdfs:label>
        <skos:definition>Organization through which a person makes a contribution to a work or instance.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/associatedWith -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/associatedWith">
        <rdfs:subPropertyOf rdf:resource="http://id.loc.gov/ontologies/bibframe/relatedTo"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:label>associated with</rdfs:label>
        <skos:definition>Event or Work that is associated in a general way with another event or work, instance or item, e.g., the Bible used at a presidential inauguration.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/composedOf -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/composedOf">
        <rdfs:label>composed of</rdfs:label>
        <skos:definition>Portion of a multi-level identifier, such as a prefix, e.g., BWV of BWV 464, or a hierarchical number, e.g., 2, of op. 45, no. 2.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/createdFor -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/createdFor">
        <rdfs:subPropertyOf rdf:resource="http://id.loc.gov/ontologies/bibframe/relatedTo"/>
        <rdfs:domain rdf:resource="http://id.loc.gov/ontologies/bibframe/Work"/>
        <rdfs:range rdf:resource="http://id.loc.gov/ontologies/bibframe/Event"/>
        <rdfs:label>created for</rdfs:label>
        <skos:definition>Event a work is created for, e.g., a motet celebrating the inauguration of a pope.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasCompiledWork -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/hasCompiledWork">
        <rdfs:domain rdf:resource="http://id.loc.gov/ontologies/bibframe/Work"/>
        <rdfs:range rdf:resource="http://id.loc.gov/ontologies/bibframe/Work"/>
        <rdfs:label>has Compiled Work</rdfs:label>
        <skos:definition>Property connecting a bf:Work or its subclasses to a bf:Work that is exactly the same except that it is in an instance of a compilation.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasDoublingMediumOfPerformance -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/hasDoublingMediumOfPerformance">
        <rdfs:range rdf:resource="http://performedmusicontology.org/ontology/MediumOfPerformance"/>
        <rdfs:label>has Doubling Medium of Performance</rdfs:label>
        <skos:definition>2 (or more) mediums of performance intended to be performed alternately by a single performer</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasDramaticRole -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/hasDramaticRole">
        <rdfs:range rdf:resource="http://performedmusicontology.org/ontology/DramaticRole"/>
        <rdfs:label>has Dramatic Role</rdfs:label>
        <skos:definition>Property connecting a bf:Contribution to the character of a performer in a dramatic work (e.g., an opera, play, video), e.g., &quot;Carmen&quot; in the opera or video &quot;Carmen&quot;.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasEventName -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/hasEventName">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://id.loc.gov/ontologies/bibframe/Event"/>
        <rdfs:label>has Event Name</rdfs:label>
        <skos:definition>Name of an event, whether formal or created for metadata purposes</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasInspiration -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/hasInspiration">
        <rdfs:subPropertyOf rdf:resource="http://id.loc.gov/ontologies/bibframe/relatedTo"/>
        <owl:inverseOf rdf:resource="http://performedmusicontology.org/ontology/inspirationFor"/>
        <rdfs:label>has as Inspiration</rdfs:label>
        <skos:definition>Event that inspired the creation of a work.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasKeyMode -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/hasKeyMode">
        <rdfs:range rdf:resource="http://performedmusicontology.org/ontology/KeyMode"/>
        <rdfs:label>has Key and Mode</rdfs:label>
        <skos:definition>Property linking Work to KeyMode.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasMedium -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/hasMedium">
        <rdfs:domain rdf:resource="http://id.loc.gov/ontologies/bibframe/Work"/>
        <rdfs:label>has Medium</rdfs:label>
        <skos:definition>Property that links a Work or Performance to the declared or performed medium.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasMediumOfPerformance -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/hasMediumOfPerformance">
        <rdfs:range rdf:resource="http://performedmusicontology.org/ontology/MediumOfPerformance"/>
        <rdfs:label>has Medium Of Performance</rdfs:label>
        <skos:definition>Specific medium of performance, e.g., bass guitar.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasMediumPart -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/hasMediumPart">
        <rdfs:range rdf:resource="http://performedmusicontology.org/ontology/MediumPart"/>
        <rdfs:label>has Medium Part</rdfs:label>
        <skos:definition>Specific element within an overall declared or performed medium, e.g., er hu, mixed chorus.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasMediumPartType -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/hasMediumPartType">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://performedmusicontology.org/ontology/MediumPart"/>
        <rdfs:range rdf:resource="http://performedmusicontology.org/ontology/MediumPartType"/>
        <rdfs:label>has Medium Part Type</rdfs:label>
        <skos:definition>Property that links a a pmo:MediumPart to a specific role or type of part, e.g., solo, ad lib, optional, offstage, amplified.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasMode -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/hasMode">
        <rdfs:range rdf:resource="http://performedmusicontology.org/ontology/Mode"/>
        <rdfs:label>has Mode</rdfs:label>
        <skos:definition>Property linking Work to Mode.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasMusicPart -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/hasMusicPart">
        <rdfs:range rdf:resource="http://performedmusicontology.org/ontology/MusicPart"/>
        <rdfs:label>has Music Part</rdfs:label>
        <skos:definition>Property linking MediumPart or bf:Contribution to the specific music parts within a single medium part e.g., Violin 1, Clarinet 2.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasPerformance -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/hasPerformance">
        <rdfs:subPropertyOf rdf:resource="http://id.loc.gov/ontologies/bibframe/eventContentOf"/>
        <owl:inverseOf rdf:resource="http://performedmusicontology.org/ontology/performanceOf"/>
        <rdfs:domain rdf:resource="http://id.loc.gov/ontologies/bibframe/Work"/>
        <rdfs:range rdf:resource="http://performedmusicontology.org/ontology/Performance"/>
        <rdfs:label>has Performance</rdfs:label>
        <skos:definition>Event that is a performance of a work</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasRecording -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/hasRecording">
        <rdfs:subPropertyOf rdf:resource="http://id.loc.gov/ontologies/bibframe/eventContent"/>
        <owl:inverseOf rdf:resource="http://performedmusicontology.org/ontology/recordingOf"/>
        <rdfs:domain rdf:resource="http://id.loc.gov/ontologies/bibframe/Event"/>
        <rdfs:range rdf:resource="http://id.loc.gov/ontologies/bibframe/Work"/>
        <rdfs:label>has Recording</rdfs:label>
        <skos:definition>Audio or video work created from the recording of event content.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasTempo -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/hasTempo">
        <rdfs:range rdf:resource="http://performedmusicontology.org/ontology/Tempo"/>
        <rdfs:label>has Tempo</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasTonalCenter -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/hasTonalCenter">
        <rdfs:range rdf:resource="http://performedmusicontology.org/ontology/TonalCenter"/>
        <rdfs:label>has Tonal Center</rdfs:label>
        <skos:definition>Property linking Work to Tonal Center</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/inspirationFor -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/inspirationFor">
        <rdfs:subPropertyOf rdf:resource="http://id.loc.gov/ontologies/bibframe/relatedTo"/>
        <rdfs:label>Inspiration for</rdfs:label>
        <skos:definition>Work that is inspired by an event.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/performanceOf -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/performanceOf">
        <rdfs:subPropertyOf rdf:resource="http://id.loc.gov/ontologies/bibframe/eventContent"/>
        <rdfs:domain rdf:resource="http://performedmusicontology.org/ontology/Performance"/>
        <rdfs:range rdf:resource="http://id.loc.gov/ontologies/bibframe/Work"/>
        <rdfs:label>Performance of</rdfs:label>
        <skos:definition>Work that is realized through a performance.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/recordingOf -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/recordingOf">
        <rdfs:subPropertyOf rdf:resource="http://id.loc.gov/ontologies/bibframe/eventContentOf"/>
        <rdfs:domain rdf:resource="http://id.loc.gov/ontologies/bibframe/Work"/>
        <rdfs:range rdf:resource="http://id.loc.gov/ontologies/bibframe/Event"/>
        <rdfs:label>Recording of</rdfs:label>
        <skos:definition>Event content of an audio or video work.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://id.loc.gov/ontologies/bibframe/date -->

    <owl:DatatypeProperty rdf:about="http://id.loc.gov/ontologies/bibframe/date"/>
    


    <!-- http://performedmusicontology.org/ontology/hasDistinctPartCount -->

    <owl:DatatypeProperty rdf:about="http://performedmusicontology.org/ontology/hasDistinctPartCount">
        <rdfs:comment>has Distinct Part Count</rdfs:comment>
        <skos:definition>Number of distinct music parts in a performed or declared medium, or medium part that is not a named ensemble, e.g, a performed work for 2 flutes and bassoon has a count of “3”.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasEnsembleCount -->

    <owl:DatatypeProperty rdf:about="http://performedmusicontology.org/ontology/hasEnsembleCount">
        <rdfs:comment>has Ensemble Count</rdfs:comment>
        <skos:definition>Number of named ensembles in a specified declared or performed medium, e.g., a string quartet has a count of “1”; a jazz combo has a count of “1”; a work for 2 choruses and orchestra has a count of “3”.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasNumberOfHands -->

    <owl:DatatypeProperty rdf:about="http://performedmusicontology.org/ontology/hasNumberOfHands">
        <rdfs:comment>has Number of Hands</rdfs:comment>
        <skos:definition>Number of hands required for performance in works where more than one performer shares an instrument (e.g., piano, 4 hands) or where an atypical number of hands is specified (e.g., viola, 1 hand).</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasOrder -->

    <owl:DatatypeProperty rdf:about="http://performedmusicontology.org/ontology/hasOrder">
        <rdfs:comment>Order a compiled work has in a compilation.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasPerformerCount -->

    <owl:DatatypeProperty rdf:about="http://performedmusicontology.org/ontology/hasPerformerCount">
        <rdfs:comment>has Performer Count</rdfs:comment>
        <skos:definition>Actual number of performers participating in a performed music event or work, e.g., a quartet for 4 serpents, performed by 8 bassoons, has a count of &quot;8&quot;.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasRequiredPerformerCount -->

    <owl:DatatypeProperty rdf:about="http://performedmusicontology.org/ontology/hasRequiredPerformerCount">
        <rdfs:comment>has Suggested Performer Count</rdfs:comment>
        <skos:definition>Minimum or suggested number of performers required to perform a declared medium, e.g., a work for a percussion ensemble for 3 performers has a a count of “3”.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasTrackNumber -->

    <owl:DatatypeProperty rdf:about="http://performedmusicontology.org/ontology/hasTrackNumber">
        <rdfs:comment>Track number(s) of a compiled audio work on a sound recording.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://performedmusicontology.org/ontology/phonogramCopyrightDate -->

    <owl:DatatypeProperty rdf:about="http://performedmusicontology.org/ontology/phonogramCopyrightDate">
        <rdfs:subPropertyOf rdf:resource="http://id.loc.gov/ontologies/bibframe/date"/>
        <rdfs:comment>Phonogram Copyright Date</rdfs:comment>
        <skos:definition>Phonogram copyright date for a sound recording. Indicated by a &quot;p&quot; or a &quot;p&quot; in a circle.</skos:definition>
    </owl:DatatypeProperty>
    


