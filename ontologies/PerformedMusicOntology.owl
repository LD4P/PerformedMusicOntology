    <!-- http://metadataregistry.org/uri/profile/regap/identifier -->

    <owl:AnnotationProperty rdf:about="http://metadataregistry.org/uri/profile/regap/identifier"/>
    


    <!-- http://purl.org/dc/elements/1.1/title -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/title"/>
    


