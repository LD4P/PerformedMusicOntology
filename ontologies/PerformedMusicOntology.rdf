<?xml version="1.0"?>
<rdf:RDF xmlns="http://performedmusicontology.org/ontology/"
     xml:base="http://performedmusicontology.org/ontology/"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:regap="http://metadataregistry.org/uri/profile/regap/"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:dc="http://purl.org/dc/elements/1.1/">
    <owl:Ontology rdf:about="http://performedmusicontology.org/ontology/">
        <owl:versionIRI rdf:resource="http://performedmusicontology.org/ontology/"/>
    </owl:Ontology>
    
    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    <!-- http://performedmusicontology.org/ontology/aggregatedIn -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/aggregatedIn">
        <owl:inverseOf rdf:resource="http://performedmusicontology.org/ontology/aggregates"/>
        <skos:definition>Property connecting an individual aggregated Instance to the Instance in which it is aggregated.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/aggregates -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/aggregates">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <skos:definition>Property connecting an aggregating instance to an individual instance that it aggregates.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/asMemberOf -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/asMemberOf">
        <rdfs:comment>Use with bf:Contribution</rdfs:comment>
        <rdfs:label>as Member of</rdfs:label>
        <skos:definition>Organization through which a person makes a contribution to a work or instance.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/associatedWith -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/associatedWith">
        <rdfs:subPropertyOf rdf:resource="http://id.loc.gov/ontologies/bibframe/relatedTo"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:comment>Example: the Bible used at a presidential inauguration</rdfs:comment>
        <rdfs:label>associated with</rdfs:label>
        <skos:definition>Event or Work that is associated in a general way with another event or work, instance or item.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/composedOf -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/composedOf">
        <rdfs:comment>Use with Thematic Identifer Statement, Opus Number Statement</rdfs:comment>
        <rdfs:label>composed of</rdfs:label>
        <skos:definition>Portion of a multi-level identifier, such as a prefix or part number</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/createdFor -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/createdFor">
        <rdfs:subPropertyOf rdf:resource="http://id.loc.gov/ontologies/bibframe/relatedTo"/>
        <rdfs:domain rdf:resource="http://id.loc.gov/ontologies/bibframe/Work"/>
        <rdfs:comment>Example: a laudatory motet intended to be sung at the inauguration of a pope.</rdfs:comment>
        <rdfs:label>created for</rdfs:label>
        <skos:definition>Event for which a work is created.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasDoublingMediumOfPerformance -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/hasDoublingMediumOfPerformance">
        <rdfs:comment>Use with MediumPart</rdfs:comment>
        <rdfs:label>has Doubling Medium of Performance</rdfs:label>
        <skos:definition>2 (or more) mediums of performance intended to be performed alternately or at the same time by a single performer</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasDramaticRole -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/hasDramaticRole">
        <rdfs:comment>Example: the role of Carmen in the opera Carmen.</rdfs:comment>
        <rdfs:label>has Dramatic Role</rdfs:label>
        <skos:definition>Property connecting a Contribution to the character of a performer in a dramatic work, such as an opera, play, or film.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasEventName -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/hasEventName">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://id.loc.gov/ontologies/bibframe/Event"/>
        <rdfs:label>has Event Name</rdfs:label>
        <skos:definition>Name of an event, whether formal or created for metadata purposes</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasInspiration -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/hasInspiration">
        <rdfs:subPropertyOf rdf:resource="http://id.loc.gov/ontologies/bibframe/relatedTo"/>
        <owl:inverseOf rdf:resource="http://performedmusicontology.org/ontology/inspirationFor"/>
        <rdfs:label>has as Inspiration</rdfs:label>
        <skos:definition>Event that inspired the creation of a work.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasKeyMode -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/hasKeyMode">
        <rdfs:comment>Example: E major, F minor</rdfs:comment>
        <rdfs:label>has key and mode</rdfs:label>
        <skos:definition>Property linking a Work to a combination of a musical key and mode.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasMedium -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/hasMedium">
        <rdfs:label>has Medium</rdfs:label>
        <skos:definition>Property that links a Work or Performance to the Declared or Performed Medium.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasMediumOfPerformance -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/hasMediumOfPerformance">
        <rdfs:comment>Examples: bass guitar; er hu; soprano; computer</rdfs:comment>
        <rdfs:label>has Medium Of Performance</rdfs:label>
        <skos:definition>Specific medium of performance.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasMediumPart -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/hasMediumPart">
        <rdfs:range rdf:resource="http://performedmusicontology.org/ontology/MediumPart"/>
        <rdfs:comment>Examples: er hu, mixed chorus</rdfs:comment>
        <rdfs:comment>Use with :DeclaredMedium or :PerformedMedium</rdfs:comment>
        <rdfs:label>has Medium Part</rdfs:label>
        <skos:definition>Specific element within an overall declared or performed medium.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasMediumPartType -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/hasMediumPartType">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://performedmusicontology.org/ontology/MediumPart"/>
        <rdfs:comment>Examples: solo, ad lib, optional, offstage, amplified</rdfs:comment>
        <rdfs:label>has Medium Part Type</rdfs:label>
        <skos:definition>Property that links a a pmo:MediumPart to a specific role or type of part.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasMode -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/hasMode">
        <rdfs:domain rdf:resource="http://id.loc.gov/ontologies/bibframe/Work"/>
        <rdfs:range rdf:resource="http://performedmusicontology.org/ontology/Mode"/>
        <rdfs:label>has Mode</rdfs:label>
        <skos:definition>Property that links a Work to a musical Mode.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasMusicPart -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/hasMusicPart">
        <rdfs:comment>Use with MediumPart</rdfs:comment>
        <rdfs:label>has Music Part</rdfs:label>
        <skos:definition>Property linking MediumPart or bf:Contribution to the specific music parts within a single medium part e.g., Violin 1, Clarinet 2.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasPerformance -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/hasPerformance">
        <rdfs:subPropertyOf rdf:resource="http://id.loc.gov/ontologies/bibframe/eventContentOf"/>
        <owl:inverseOf rdf:resource="http://performedmusicontology.org/ontology/performanceOf"/>
        <rdfs:range rdf:resource="http://performedmusicontology.org/ontology/Performance"/>
        <rdfs:label>has Performance</rdfs:label>
        <skos:definition>Event that is a performance, either improvised or of a pre-existing work.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasRecording -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/hasRecording">
        <rdfs:subPropertyOf rdf:resource="http://id.loc.gov/ontologies/bibframe/eventContent"/>
        <owl:inverseOf rdf:resource="http://performedmusicontology.org/ontology/recordingOf"/>
        <rdfs:domain rdf:resource="http://id.loc.gov/ontologies/bibframe/Event"/>
        <rdfs:range rdf:resource="http://id.loc.gov/ontologies/bibframe/Work"/>
        <rdfs:label>has Recording</rdfs:label>
        <skos:definition>Audio or video work created from the recording of event content.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasTempo -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/hasTempo">
        <rdfs:label>has Tempo</rdfs:label>
        <skos:definition>The speed or pace of a music work, whether declared (in a score) or performed.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasTonalCenter -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/hasTonalCenter">
        <rdfs:label>has Tonal Center</rdfs:label>
        <skos:definition>Property that links Work to its Tonal Center</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/inspirationFor -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/inspirationFor">
        <rdfs:subPropertyOf rdf:resource="http://id.loc.gov/ontologies/bibframe/relatedTo"/>
        <rdfs:label>Inspiration for</rdfs:label>
        <skos:definition>Work that is inspired by an event.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/performanceOf -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/performanceOf">
        <rdfs:subPropertyOf rdf:resource="http://id.loc.gov/ontologies/bibframe/eventContent"/>
        <rdfs:domain rdf:resource="http://performedmusicontology.org/ontology/Performance"/>
        <rdfs:range rdf:resource="http://id.loc.gov/ontologies/bibframe/Work"/>
        <rdfs:label>Performance of</rdfs:label>
        <skos:definition>Work that is realized through a performance.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://performedmusicontology.org/ontology/recordingOf -->

    <owl:ObjectProperty rdf:about="http://performedmusicontology.org/ontology/recordingOf">
        <rdfs:subPropertyOf rdf:resource="http://id.loc.gov/ontologies/bibframe/eventContentOf"/>
        <rdfs:domain rdf:resource="http://id.loc.gov/ontologies/bibframe/Work"/>
        <rdfs:range rdf:resource="http://id.loc.gov/ontologies/bibframe/Event"/>
        <rdfs:label>Recording of</rdfs:label>
        <skos:definition>Audio or video work created from the recording of event content.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    

    <!-- http://performedmusicontology.org/ontology/hasDistinctPartCount -->

    <owl:DatatypeProperty rdf:about="http://performedmusicontology.org/ontology/hasDistinctPartCount">
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment>Example: A work for 2 flutes has a distinct part count of 2.</rdfs:comment>
        <rdfs:label>Number of distinct parts</rdfs:label>
        <skos:definition>Number of distinct music parts in a Performed Medium, Declared Medium, or Medium Part that is not a named ensemble.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasEnsembleCount -->

    <owl:DatatypeProperty rdf:about="http://performedmusicontology.org/ontology/hasEnsembleCount">
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment>Example: a work for 2 choruses and orchestra has an ensemble count of 3.</rdfs:comment>
        <rdfs:label>Number of ensembles</rdfs:label>
        <skos:definition>Number of named ensembles in a specified Declared Medium, Performed Medium, or Medium Part.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasNumberOfHands -->

    <owl:DatatypeProperty rdf:about="http://performedmusicontology.org/ontology/hasNumberOfHands">
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:label>Number of hands</rdfs:label>
        <skos:definition>Number of hands required for performance in works where more than one performer shares an instrument, such as piano, 4 hands, or where an atypical number of hands is specified, such as viola, 1 hand.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasOrder -->

    <owl:DatatypeProperty rdf:about="http://performedmusicontology.org/ontology/hasOrder">
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:label>Order in sequence</rdfs:label>
        <skos:definition>Order an aggregated instance appears in an aggregating instance, or the order a work component appears in a work.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasPerformerCount -->

    <owl:DatatypeProperty rdf:about="http://performedmusicontology.org/ontology/hasPerformerCount">
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:label>Number of performers</rdfs:label>
        <skos:definition>Actual number of individual performers performing a Declared Medium, Performed Medium, Medium Part, or Music Part.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasRequiredPerformerCount -->

    <owl:DatatypeProperty rdf:about="http://performedmusicontology.org/ontology/hasRequiredPerformerCount">
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment>Example: A string quartet has a required performer count of 4.</rdfs:comment>
        <rdfs:label>Suggested number of performers</rdfs:label>
        <skos:definition>Minimum or suggested number of performers required to perform a Declared Medium, Performed Medium, Medium Part, or Music Part.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://performedmusicontology.org/ontology/hasTrackNumber -->

    <owl:DatatypeProperty rdf:about="http://performedmusicontology.org/ontology/hasTrackNumber">
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:label>Track number</rdfs:label>
        <skos:definition>Track number(s) of a compiled audio work on a sound recording.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://performedmusicontology.org/ontology/phonogramCopyrightDate -->

    <owl:DatatypeProperty rdf:about="http://performedmusicontology.org/ontology/phonogramCopyrightDate">
        <rdfs:subPropertyOf rdf:resource="http://id.loc.gov/ontologies/bibframe/date"/>
        <rdfs:label>Phonogram copyright date</rdfs:label>
        <skos:definition>Phonogram copyright date for a sound recording.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    
    <!-- http://performedmusicontology.org/ontology/Audition -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/Audition">
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Event"/>
        <rdfs:label>Audition</rdfs:label>
        <skos:definition>Event in which a performer or actor is applying for a position in an ensemble, cast, film, etc.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/BenefitConcert -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/BenefitConcert">
        <rdfs:subClassOf rdf:resource="http://performedmusicontology.org/ontology/Concert"/>
        <skos:definition>Concert held for charitable purposes, i.e., to raise funds or awareness</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/Ceremony -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/Ceremony">
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Event"/>
        <rdfs:label>Ceremony</rdfs:label>
        <skos:definition>Ritual event, performed on special occasions (e.g., death, baptism, wedding, graduation, equinox, coronation, victory)</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/CommandPerformance -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/CommandPerformance">
        <rdfs:subClassOf rdf:resource="http://performedmusicontology.org/ontology/Concert"/>
        <rdfs:label>Command performance</rdfs:label>
        <skos:definition>Concert or Performance done at the request of a monarch or other head of state.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/Concert -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/Concert">
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Event"/>
        <rdfs:label>Concert</rdfs:label>
        <skos:definition>Live musical performance of one or more works in front of an audience or in a single recording session</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/ConcertSeries -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/ConcertSeries">
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Event"/>
        <rdfs:label>Concert series</rdfs:label>
        <skos:definition>Group of concerts in the same geographic area by a single performer or brought together by a common theme, usually available for sale as a package. e.g., an orchestra concert season, a series featuring chamber ensembles.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/ConcertTour -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/ConcertTour">
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Event"/>
        <rdfs:label>Concert tour</rdfs:label>
        <skos:definition>Series of concerts by a performer or group of performers in different cities, countries, or locations.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/DeclaredMedium -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/DeclaredMedium">
        <rdfs:label>Declared medium</rdfs:label>
        <skos:definition>Intended overall medium of performance of a musical work, as declared by the composer, a reference source, or other source.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/DiscCutting -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/DiscCutting">
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/SoundCharacteristic"/>
        <rdfs:label>Disc cutting process</rdfs:label>
        <skos:definition>Recording process in which a stylus cuts a vertical or lateral groove into an audio disc.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/Discogs -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/Discogs">
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Identifier"/>
        <rdfs:label>Discogs ID</rdfs:label>
        <skos:definition>Identifier from Discogs.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/DramaticRole -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/DramaticRole">
        <rdfs:comment>Example: the role of Carmen in the opera Carmen.</rdfs:comment>
        <rdfs:label>Dramatic role</rdfs:label>
        <skos:definition>Character of a performer in a dramatic work, such as an opera or play, as realized in a sound recording, a video, in text, etc.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/EnsembleMediumOfPerformance -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/EnsembleMediumOfPerformance">
        <rdfs:subClassOf rdf:resource="http://performedmusicontology.org/ontology/MediumOfPerformance"/>
        <rdfs:comment>Examples: orchestra, jazz combo, string quartet, percussion ensemble</rdfs:comment>
        <rdfs:label>Ensemble type</rdfs:label>
        <skos:definition>Named group of like or unlike mediums of performance intended to be played by multiple performers.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/EventName -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/EventName">
        <rdfs:label>Event name</rdfs:label>
        <skos:definition>Name given to an event.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/FirstPerformance -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/FirstPerformance">
        <rdfs:subClassOf rdf:resource="http://performedmusicontology.org/ontology/Performance"/>
        <rdfs:label>First performance</rdfs:label>
        <skos:definition>The first known performance of a work.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/IndividualMediumOfPerformance -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/IndividualMediumOfPerformance">
        <rdfs:subClassOf rdf:resource="http://performedmusicontology.org/ontology/MediumOfPerformance"/>
        <rdfs:comment>Examples: flute, keyboard, pi pa, typewriter</rdfs:comment>
        <rdfs:comment>Individual musical instrument, voice, or other object used as a medium of performance.</rdfs:comment>
        <rdfs:label>Individual medium of performance</rdfs:label>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/KeyMode -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/KeyMode">
        <rdfs:comment>Examples: C major; B-flat minor</rdfs:comment>
        <rdfs:label>Music key and mode</rdfs:label>
        <skos:definition>Key and mode of a music work when used in combination.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/LivePerformance -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/LivePerformance">
        <rdfs:subClassOf rdf:resource="http://performedmusicontology.org/ontology/Performance"/>
        <rdfs:label>Live performance</rdfs:label>
        <skos:definition>Live Performance that is recorded at a public event or in a single recording session.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/MasterClass -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/MasterClass">
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/Event"/>
        <rdfs:label>Master class</rdfs:label>
        <skos:definition>Event in which a recognized performer critiques the performances of one or more other performers.</skos:definition>
    </owl:Class>
    


    <!-- http://performedmusicontology.org/ontology/MediumOfPerformance -->

    <owl:Class rdf:about="http://performedmusicontology.org/ontology/MediumOfPerformance">
        <rdfs:label>Medium of performance</rdfs:label>
        <skos:definition>Instruments, voices, ensembles, choruses, and other participants (actors, dancers, etc.) required for, or performing in, an event or work, including electronics, computers, installations, etc.</skos:definition>
    </owl:Class>
    


