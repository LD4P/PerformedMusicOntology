   <rdf:Description rdf:about="http://performedmusicontology.org/ontologies/vocabularies/playing_speed/">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
      <rdfs:label xml:lang="en">PMO Playing Speed Vocabulary</rdfs:label>
      <dcterms:title xml:lang="en">PMO Playing Speed Vocabulary</dcterms:title>
      <skos:definition xml:lang="en">The PMO Playing Speed Vocabulary is a controlled vocabulary for playing speeds of audio recordings. Terms are derived from the MARC21 bibliographic format 007/?? codes. It was developed for the Performed Music Ontology, but may be used with other ontologies.</skos:definition>
   </rdf:Description>
   
   <rdf:Description rdf:about="http://performedmusicontology.org/ontologies/vocabularies/playing_speed/fpm_12">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:prefLabel xml:lang="en">12 fpm</skos:prefLabel>
      <skos:definition xml:lang="en">Playing speed of 12 feet per minute x 10.</skos:definition>
      <rdfs:comment xml:lang="en">Used for piano and organ rolls.</rdfs:comment>
      <skos:inScheme rdf:resource="http://performedmusicontology.org/ontologies/vocabularies/playing_speed/"/>
      <skos:changeNote rdf:parseType="Resource">
         <rdf:value>NEW</rdf:value>
         <dcterms:date>2018-07-01</dcterms:date>
      </skos:changeNote>
   </rdf:Description>
   
  <rdf:Description rdf:about="http://performedmusicontology.org/ontologies/vocabularies/playing_speed/ips.9">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:prefLabel xml:lang="en">15/16 ips</skos:prefLabel>
      <skos:altLabel xml:lang="en">Advanced Audio Recording format</skos:altLabel>
      <skos:definition xml:lang="en">Playing speed of 15/16 inches per second.</skos:definition>
      <rdfs:comment xml:lang="en">Speed occurs on half-speed mini-cassettes used in small portable recorders.</rdfs:comment>
      <skos:inScheme rdf:resource="http://performedmusicontology.org/ontologies/vocabularies/playing_speed/"/>
      <skos:changeNote rdf:parseType="Resource">
         <rdf:value>NEW</rdf:value>
         <dcterms:date>2018-07-01</dcterms:date>
      </skos:changeNote>
   </rdf:Description>
  
