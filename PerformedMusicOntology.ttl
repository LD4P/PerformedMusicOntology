@prefix : <http://performedmusicontology.org/ontology/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix regap: <http://metadataregistry.org/uri/profile/regap/> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix rdakit: <http://metadataregistry.org/uri/profile/rdakit/> .
@base <http://performedmusicontology.org/ontology/> .

<http://performedmusicontology.org/ontology/> rdf:type owl:Ontology ;
                                               owl:versionIRI <http://performedmusicontology.org/ontology/> .

#################################################################
#    Annotation properties
#################################################################

###  http://metadataregistry.org/uri/profile/rdakit/toolkitDefinition
rdakit:toolkitDefinition rdf:type owl:AnnotationProperty .


###  http://metadataregistry.org/uri/profile/rdakit/toolkitLabel
rdakit:toolkitLabel rdf:type owl:AnnotationProperty .


###  http://metadataregistry.org/uri/profile/regap/identifier
regap:identifier rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#altLabel
skos:altLabel rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#broader
skos:broader rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#definition
skos:definition rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#inScheme
skos:inScheme rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#narrower
skos:narrower rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#prefLabel
skos:prefLabel rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#scopeNote
skos:scopeNote rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://id.loc.gov/ontologies/bibframe/eventContent
<http://id.loc.gov/ontologies/bibframe/eventContent> rdf:type owl:ObjectProperty .


###  http://id.loc.gov/ontologies/bibframe/eventContentOf
<http://id.loc.gov/ontologies/bibframe/eventContentOf> rdf:type owl:ObjectProperty .


###  http://id.loc.gov/ontologies/bibframe/relatedTo
<http://id.loc.gov/ontologies/bibframe/relatedTo> rdf:type owl:ObjectProperty .


###  http://performedmusicontology.org/ontology/asMemberOf
:asMemberOf rdf:type owl:ObjectProperty ;
            rdfs:domain <http://id.loc.gov/ontologies/bibframe/Contribution> ;
            rdfs:range <http://id.loc.gov/ontologies/bibframe/Organization> ;
            rdfs:label "as Member of" ;
            skos:definition "Organization through which a person makes a contribution to a work or instance." .


###  http://performedmusicontology.org/ontology/associatedWith
:associatedWith rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf <http://id.loc.gov/ontologies/bibframe/relatedTo> ;
                rdf:type owl:SymmetricProperty ;
                rdfs:label "associated with" ;
                skos:definition "Event or Work that is associated in a general way with another event or work, instance or item, e.g., the Bible used at a presidential inauguration." .


###  http://performedmusicontology.org/ontology/composedOf
:composedOf rdf:type owl:ObjectProperty ;
            rdfs:label "composed of" ;
            skos:definition "Portion of a multi-level identifier, such as a prefix, e.g., BWV of BWV 464, or a hierarchical number, e.g., 2, of op. 45, no. 2." .


###  http://performedmusicontology.org/ontology/createdFor
:createdFor rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf <http://id.loc.gov/ontologies/bibframe/relatedTo> ;
            rdfs:domain <http://id.loc.gov/ontologies/bibframe/Work> ;
            rdfs:range <http://id.loc.gov/ontologies/bibframe/Event> ;
            rdfs:label "created for" ;
            skos:definition "Event a work is created for, e.g., a motet celebrating the inauguration of a pope." .


###  http://performedmusicontology.org/ontology/hasCompiledWork
:hasCompiledWork rdf:type owl:ObjectProperty ;
                 rdfs:domain <http://id.loc.gov/ontologies/bibframe/Work> ;
                 rdfs:range <http://id.loc.gov/ontologies/bibframe/Work> ;
                 rdfs:label "has Compiled Work" ;
                 skos:definition "Property connecting a bf:Work or its subclasses to a bf:Work that is exactly the same except that it is in an instance of a compilation." .


###  http://performedmusicontology.org/ontology/hasDoublingMediumOfPerformance
:hasDoublingMediumOfPerformance rdf:type owl:ObjectProperty ;
                                rdfs:range :MediumOfPerformance ;
                                rdfs:label "has Doubling Medium of Performance" ;
                                skos:definition "2 (or more) mediums of performance intended to be performed alternately by a single performer" .


###  http://performedmusicontology.org/ontology/hasDramaticRole
:hasDramaticRole rdf:type owl:ObjectProperty ;
                 rdfs:range :DramaticRole ;
                 rdfs:label "has Dramatic Role" ;
                 skos:definition "Property connecting a bf:Contribution to the character of a performer in a dramatic work (e.g., an opera, play, video), e.g., \"Carmen\" in the opera or video \"Carmen\"." .


###  http://performedmusicontology.org/ontology/hasEventName
:hasEventName rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdfs:domain <http://id.loc.gov/ontologies/bibframe/Event> ;
              rdfs:label "has Event Name" ;
              skos:definition "Name of an event, whether formal or created for metadata purposes" .


###  http://performedmusicontology.org/ontology/hasInspiration
:hasInspiration rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf <http://id.loc.gov/ontologies/bibframe/relatedTo> ;
                owl:inverseOf :inspirationFor ;
                rdfs:label "has as Inspiration" ;
                skos:definition "Event that inspired the creation of a work." .


###  http://performedmusicontology.org/ontology/hasKeyMode
:hasKeyMode rdf:type owl:ObjectProperty ;
            rdfs:range :KeyMode ;
            rdfs:label "has Key and Mode" ;
            skos:definition "Property linking Work to KeyMode." .


###  http://performedmusicontology.org/ontology/hasMedium
:hasMedium rdf:type owl:ObjectProperty ;
           rdfs:domain <http://id.loc.gov/ontologies/bibframe/Work> ;
           rdfs:label "has Medium" ;
           skos:definition "Property that links a Work or Performance to the declared or performed medium." .


###  http://performedmusicontology.org/ontology/hasMediumOfPerformance
:hasMediumOfPerformance rdf:type owl:ObjectProperty ;
                        rdfs:range :MediumOfPerformance ;
                        rdfs:label "has Medium Of Performance" ;
                        skos:definition "Specific medium of performance, e.g., bass guitar." .


###  http://performedmusicontology.org/ontology/hasMediumPart
:hasMediumPart rdf:type owl:ObjectProperty ;
               rdfs:range :MediumPart ;
               rdfs:label "has Medium Part" ;
               skos:definition "Specific element within an overall declared or performed medium, e.g., er hu, mixed chorus." .


###  http://performedmusicontology.org/ontology/hasMediumPartType
:hasMediumPartType rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf owl:topObjectProperty ;
                   rdfs:domain :MediumPart ;
                   rdfs:range :MediumPartType ;
                   rdfs:label "has Medium Part Type" ;
                   skos:definition "Property that links a a pmo:MediumPart to a specific role or type of part, e.g., solo, ad lib, optional, offstage, amplified." .


###  http://performedmusicontology.org/ontology/hasMode
:hasMode rdf:type owl:ObjectProperty ;
         rdfs:range :Mode ;
         rdfs:label "has Mode" ;
         skos:definition "Property linking Work to Mode." .


###  http://performedmusicontology.org/ontology/hasMusicPart
:hasMusicPart rdf:type owl:ObjectProperty ;
              rdfs:range :MusicPart ;
              rdfs:label "has Music Part" ;
              skos:definition "Property linking MediumPart or bf:Contribution to the specific music parts within a single medium part e.g., Violin 1, Clarinet 2." .


###  http://performedmusicontology.org/ontology/hasPerformance
:hasPerformance rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf <http://id.loc.gov/ontologies/bibframe/eventContentOf> ;
                owl:inverseOf :performanceOf ;
                rdfs:domain <http://id.loc.gov/ontologies/bibframe/Work> ;
                rdfs:range :Performance ;
                rdfs:label "has Performance" ;
                skos:definition "Event that is a performance of a work" .


###  http://performedmusicontology.org/ontology/hasRecording
:hasRecording rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf <http://id.loc.gov/ontologies/bibframe/eventContent> ;
              owl:inverseOf :recordingOf ;
              rdfs:domain <http://id.loc.gov/ontologies/bibframe/Event> ;
              rdfs:range <http://id.loc.gov/ontologies/bibframe/Work> ;
              rdfs:label "has Recording" ;
              skos:definition "Audio or video work created from the recording of event content." .


###  http://performedmusicontology.org/ontology/hasTempo
:hasTempo rdf:type owl:ObjectProperty ;
          rdfs:range :Tempo ;
          rdfs:label "has Tempo" .


###  http://performedmusicontology.org/ontology/hasTonalCenter
:hasTonalCenter rdf:type owl:ObjectProperty ;
                rdfs:range :TonalCenter ;
                rdfs:label "has Tonal Center" ;
                skos:definition "Property linking Work to Tonal Center" .


###  http://performedmusicontology.org/ontology/inspirationFor
:inspirationFor rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf <http://id.loc.gov/ontologies/bibframe/relatedTo> ;
                rdfs:label "Inspiration for" ;
                skos:definition "Work that is inspired by an event." .


###  http://performedmusicontology.org/ontology/performanceOf
:performanceOf rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf <http://id.loc.gov/ontologies/bibframe/eventContent> ;
               rdfs:domain :Performance ;
               rdfs:range <http://id.loc.gov/ontologies/bibframe/Work> ;
               rdfs:label "Performance of" ;
               skos:definition "Work that is realized through a performance." .


###  http://performedmusicontology.org/ontology/recordingOf
:recordingOf rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf <http://id.loc.gov/ontologies/bibframe/eventContentOf> ;
             rdfs:domain <http://id.loc.gov/ontologies/bibframe/Work> ;
             rdfs:range <http://id.loc.gov/ontologies/bibframe/Event> ;
             rdfs:label "Recording of" ;
             skos:definition "Event content of an audio or video work." .


#################################################################
#    Data properties
#################################################################

###  http://id.loc.gov/ontologies/bibframe/date
<http://id.loc.gov/ontologies/bibframe/date> rdf:type owl:DatatypeProperty .


###  http://performedmusicontology.org/ontology/hasDistinctPartCount
:hasDistinctPartCount rdf:type owl:DatatypeProperty ;
                      rdfs:comment "has Distinct Part Count" ;
                      skos:definition "Number of distinct music parts in a performed or declared medium, or medium part that is not a named ensemble, e.g, a performed work for 2 flutes and bassoon has a count of “3”." .


###  http://performedmusicontology.org/ontology/hasEnsembleCount
:hasEnsembleCount rdf:type owl:DatatypeProperty ;
                  rdfs:comment "has Ensemble Count" ;
                  skos:definition "Number of named ensembles in a specified declared or performed medium, e.g., a string quartet has a count of “1”; a jazz combo has a count of “1”; a work for 2 choruses and orchestra has a count of “3”." .


###  http://performedmusicontology.org/ontology/hasNumberOfHands
:hasNumberOfHands rdf:type owl:DatatypeProperty ;
                  rdfs:comment "has Number of Hands" ;
                  skos:definition "Number of hands required for performance in works where more than one performer shares an instrument (e.g., piano, 4 hands) or where an atypical number of hands is specified (e.g., viola, 1 hand)." .


###  http://performedmusicontology.org/ontology/hasOrder
:hasOrder rdf:type owl:DatatypeProperty ;
          rdfs:comment "Order a compiled work has in a compilation." .


###  http://performedmusicontology.org/ontology/hasPerformerCount
:hasPerformerCount rdf:type owl:DatatypeProperty ;
                   rdfs:comment "has Performer Count" ;
                   skos:definition "Actual number of performers participating in a performed music event or work, e.g., a quartet for 4 serpents, performed by 8 bassoons, has a count of \"8\"." .


###  http://performedmusicontology.org/ontology/hasRequiredPerformerCount
:hasRequiredPerformerCount rdf:type owl:DatatypeProperty ;
                           rdfs:comment "has Suggested Performer Count" ;
                           skos:definition "Minimum or suggested number of performers required to perform a declared medium, e.g., a work for a percussion ensemble for 3 performers has a a count of “3”." .


###  http://performedmusicontology.org/ontology/hasTrackNumber
:hasTrackNumber rdf:type owl:DatatypeProperty ;
                rdfs:comment "Track number(s) of a compiled audio work on a sound recording." .


###  http://performedmusicontology.org/ontology/phonogramCopyrightDate
:phonogramCopyrightDate rdf:type owl:DatatypeProperty ;
                        rdfs:subPropertyOf <http://id.loc.gov/ontologies/bibframe/date> ;
                        rdfs:comment "Phonogram Copyright Date" ;
                        skos:definition "Phonogram copyright date for a sound recording. Indicated by a \"p\" or a \"p\" in a circle." .


#################################################################
#    Classes
#################################################################

###  http://erlangen-crm.org/efrbroo/F31_Performance
<http://erlangen-crm.org/efrbroo/F31_Performance> rdf:type owl:Class .


###  http://id.loc.gov/ontologies/bibframe/Carrier
<http://id.loc.gov/ontologies/bibframe/Carrier> rdf:type owl:Class .


###  http://id.loc.gov/ontologies/bibframe/Contribution
<http://id.loc.gov/ontologies/bibframe/Contribution> rdf:type owl:Class .


###  http://id.loc.gov/ontologies/bibframe/EncodingFormat
<http://id.loc.gov/ontologies/bibframe/EncodingFormat> rdf:type owl:Class .


###  http://id.loc.gov/ontologies/bibframe/Event
<http://id.loc.gov/ontologies/bibframe/Event> rdf:type owl:Class .


###  http://id.loc.gov/ontologies/bibframe/Identifier
<http://id.loc.gov/ontologies/bibframe/Identifier> rdf:type owl:Class .


###  http://id.loc.gov/ontologies/bibframe/Organization
<http://id.loc.gov/ontologies/bibframe/Organization> rdf:type owl:Class .


###  http://id.loc.gov/ontologies/bibframe/PlayingSpeed
<http://id.loc.gov/ontologies/bibframe/PlayingSpeed> rdf:type owl:Class .


###  http://id.loc.gov/ontologies/bibframe/SoundCharacteristic
<http://id.loc.gov/ontologies/bibframe/SoundCharacteristic> rdf:type owl:Class .


###  http://id.loc.gov/ontologies/bibframe/TapeConfig
<http://id.loc.gov/ontologies/bibframe/TapeConfig> rdf:type owl:Class .


###  http://id.loc.gov/ontologies/bibframe/Work
<http://id.loc.gov/ontologies/bibframe/Work> rdf:type owl:Class .


###  http://performedmusicontology.org/ontology/Audition
:Audition rdf:type owl:Class ;
          rdfs:subClassOf <http://id.loc.gov/ontologies/bibframe/Event> ;
          skos:definition "Event in which a performer or actor is applying for a position in an ensemble, cast, film, etc." .


###  http://performedmusicontology.org/ontology/BenefitConcert
:BenefitConcert rdf:type owl:Class ;
                rdfs:subClassOf :Concert ;
                skos:definition "Concert held for charitable purposes, i.e., to raise funds or awareness" .


###  http://performedmusicontology.org/ontology/Ceremony
:Ceremony rdf:type owl:Class ;
          rdfs:subClassOf <http://id.loc.gov/ontologies/bibframe/Event> ;
          skos:definition "Ritual event, performed on special occasions (e.g., death, baptism, wedding, graduation, equinox, coronation, victory)" .


###  http://performedmusicontology.org/ontology/CommandPerformance
:CommandPerformance rdf:type owl:Class ;
                    rdfs:subClassOf :Concert ;
                    skos:definition "Performance done at the request of a monarch or other head of state." .


###  http://performedmusicontology.org/ontology/Concert
:Concert rdf:type owl:Class ;
         rdfs:subClassOf <http://id.loc.gov/ontologies/bibframe/Event> ;
         skos:definition "Live musical performance of one or more works in front of an audience" .


###  http://performedmusicontology.org/ontology/ConcertSeries
:ConcertSeries rdf:type owl:Class ;
               rdfs:subClassOf <http://id.loc.gov/ontologies/bibframe/Event> ;
               skos:definition "Group of concerts in the same geographic area by a single performer or brought together by a common theme, usually available for sale as a package. e.g., an orchestra concert season, a series featuring chamber ensembles." .


###  http://performedmusicontology.org/ontology/ConcertTour
:ConcertTour rdf:type owl:Class ;
             rdfs:subClassOf <http://id.loc.gov/ontologies/bibframe/Event> ;
             skos:definition "Series of concerts by a performer or group of performers in different cities, countries, or locations." .


###  http://performedmusicontology.org/ontology/DeclaredMedium
:DeclaredMedium rdf:type owl:Class ;
                rdfs:label "Declared medium" ;
                skos:definition "Intended overall medium of performance of a musical work, as declared by the composer, a reference source, or other source." .


###  http://performedmusicontology.org/ontology/DiscCutting
:DiscCutting rdf:type owl:Class ;
             rdfs:subClassOf <http://id.loc.gov/ontologies/bibframe/SoundCharacteristic> ;
             rdfs:label "Disc cutting process" ;
             skos:definition "Recording process in which a stylus cuts a vertical or lateral groove into an audio disc." .


###  http://performedmusicontology.org/ontology/Discogs
:Discogs rdf:type owl:Class ;
         rdfs:subClassOf <http://id.loc.gov/ontologies/bibframe/Identifier> ;
         rdfs:label "Discogs ID" ;
         skos:definition "Identifier from Discogs." .


###  http://performedmusicontology.org/ontology/DramaticRole
:DramaticRole rdf:type owl:Class ;
              rdfs:label "Dramatic role" ;
              skos:definition "Character of a performer in a dramatic work (e.g., an opera, play, video), e.g., \"Carmen\" in the opera or video \"Carmen\"." .


###  http://performedmusicontology.org/ontology/EnsembleMediumOfPerformance
:EnsembleMediumOfPerformance rdf:type owl:Class ;
                             rdfs:subClassOf :MediumOfPerformance ;
                             skos:definition "Named group of like or unlike mediums of performance intended to be played by multiple performers, e.g., orchestra, jazz combo, string quartet, percussion ensemble." .


###  http://performedmusicontology.org/ontology/EventName
:EventName rdf:type owl:Class ;
           rdfs:label "Event name" ;
           skos:definition "Name given to an event." .


###  http://performedmusicontology.org/ontology/FirstPerformance
:FirstPerformance rdf:type owl:Class ;
                  rdfs:subClassOf :Performance ;
                  skos:definition "The world premiere performance of a work." .


###  http://performedmusicontology.org/ontology/IndividualMediumOfPerformance
:IndividualMediumOfPerformance rdf:type owl:Class ;
                               rdfs:subClassOf :MediumOfPerformance ;
                               rdfs:comment "Individual musical instrument, voice, or other object used as a medium of performance (e.g., flute, keyboards, pi pa, drinking glass, typewriter)." .


###  http://performedmusicontology.org/ontology/KeyMode
:KeyMode rdf:type owl:Class ;
         rdfs:label "Music key and mode" ;
         skos:definition "Key and mode of a music work when used in combination, e.g., C major, B-flat major." .


###  http://performedmusicontology.org/ontology/LivePerformance
:LivePerformance rdf:type owl:Class ;
                 rdfs:subClassOf :Performance ;
                 skos:definition "Performance that is recorded at a concert." .


###  http://performedmusicontology.org/ontology/MasterClass
:MasterClass rdf:type owl:Class ;
             rdfs:subClassOf <http://id.loc.gov/ontologies/bibframe/Event> ;
             skos:definition "Event in which a recognized performer critiques the performances of one or more other performers." .


###  http://performedmusicontology.org/ontology/MediumOfPerformance
:MediumOfPerformance rdf:type owl:Class ;
                     rdfs:label "Medium of performance" ;
                     skos:definition "Instruments, voices, ensembles, choruses, and other participants (actors, dancers, etc.) required for, or performing in, an event or work, including electronics, computers, installations, etc." .


###  http://performedmusicontology.org/ontology/MediumPart
:MediumPart rdf:type owl:Class ;
            rdfs:comment "Use with DeclaredMedium, PerformedMedium" ;
            rdfs:label "Medium part" ;
            skos:definition "Medium of performance, type, etc., of a specific part of the declared or performed medium." .


###  http://performedmusicontology.org/ontology/MediumPartType
:MediumPartType rdf:type owl:Class ;
                rdfs:label "Part type" ;
                skos:definition "Specific role or type of a medium part, e.g., solo, ad lib, optional, offstage, amplified" .


###  http://performedmusicontology.org/ontology/Mode
:Mode rdf:type owl:Class ;
      rdfs:label "Music mode" ;
      skos:definition "Musical mode of a music work, e.g., Dorian, Dhaivata." .


###  http://performedmusicontology.org/ontology/MusicBrainz
:MusicBrainz rdf:type owl:Class ;
             rdfs:subClassOf <http://id.loc.gov/ontologies/bibframe/Identifier> ;
             rdfs:label "MusicBrainz ID" ;
             skos:definition "Identifier from MusicBrainz" .


###  http://performedmusicontology.org/ontology/MusicPart
:MusicPart rdf:type owl:Class ;
           rdfs:label "Music part" ;
           skos:definition "Specific element within an overall declared or performed medium, e.g., Violin 1, Trumpet 2." .


###  http://performedmusicontology.org/ontology/OpenMicPerformance
:OpenMicPerformance rdf:type owl:Class ;
                    rdfs:subClassOf :Performance ;
                    skos:definition "Live performance at a coffee house, night club, or other venue by amateur performers." .


###  http://performedmusicontology.org/ontology/OpusNumber
:OpusNumber rdf:type owl:Class ;
            rdfs:subClassOf <http://id.loc.gov/ontologies/bibframe/Identifier> ;
            rdfs:label "Opus number" ;
            skos:definition "Opus number for a   musical work, e.g. “45” of Op. 45. No. 1" .


###  http://performedmusicontology.org/ontology/OpusNumberPart
:OpusNumberPart rdf:type owl:Class ;
                rdfs:subClassOf <http://id.loc.gov/ontologies/bibframe/Identifier> ;
                rdfs:label "Opus number part" ;
                skos:definition "Number for a   portion of an opus number, e.g., no. 1   of Op. 45, no. 1." .


###  http://performedmusicontology.org/ontology/OpusNumberStatement
:OpusNumberStatement rdf:type owl:Class ;
                     rdfs:subClassOf <http://id.loc.gov/ontologies/bibframe/Identifier> ;
                     rdfs:label "Opus number statement" ;
                     skos:definition "Statement about the opus number for a musical work." .


###  http://performedmusicontology.org/ontology/Performance
:Performance rdf:type owl:Class ;
             rdfs:subClassOf <http://erlangen-crm.org/efrbroo/F31_Performance> ,
                             <http://id.loc.gov/ontologies/bibframe/Event> ;
             skos:definition "Event in which a performer or group of performers present a work to an audience or in a recording studio." .


###  http://performedmusicontology.org/ontology/PerformedMedium
:PerformedMedium rdf:type owl:Class ;
                 rdfs:label "Performed medium" ;
                 skos:definition "The overall medium of performance used in the performance of a musical work. This may or may not be the same as the declared medium of performance." .


###  http://performedmusicontology.org/ontology/RecordingSession
:RecordingSession rdf:type owl:Class ;
                  rdfs:subClassOf <http://id.loc.gov/ontologies/bibframe/Event> ;
                  skos:definition "Event in which one or more performances are recorded with the aim of producing a recording for issue." .


###  http://performedmusicontology.org/ontology/Rehearsal
:Rehearsal rdf:type owl:Class ;
           rdfs:subClassOf <http://id.loc.gov/ontologies/bibframe/Event> ;
           skos:definition "Event in which a performer or group of performers prepare for a performance in a concert." .


###  http://performedmusicontology.org/ontology/RismNumber
:RismNumber rdf:type owl:Class ;
            rdfs:subClassOf <http://id.loc.gov/ontologies/bibframe/Identifier> ;
            rdfs:label "RISM number" ;
            skos:definition "Number from the Repertoire Internationale des Sources Musicales (RISM) online version." .


###  http://performedmusicontology.org/ontology/Tempo
:Tempo rdf:type owl:Class ;
       rdfs:label "Tempo" ;
       skos:definition "The speed or pace of a music work, whether declared (in a score) or performed." .


###  http://performedmusicontology.org/ontology/ThematicCatalogNumber
:ThematicCatalogNumber rdf:type owl:Class ;
                       rdfs:subClassOf <http://id.loc.gov/ontologies/bibframe/Identifier> ;
                       rdfs:label "Thematic catalog number" ;
                       skos:definition "Numeric or alphanumericdesignation of a   musical work as found in a thematic index for the composer." .


###  http://performedmusicontology.org/ontology/ThematicCatalogPrefix
:ThematicCatalogPrefix rdf:type owl:Class ;
                       rdfs:subClassOf <http://id.loc.gov/ontologies/bibframe/Identifier> ;
                       rdfs:label "Thematic catalog prefix" ;
