    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">ad lib</skos:prefLabel>
    <skos:altLabel xml:lang="en">ad libitum</skos:altLabel>
    <skos:definition xml:lang="en">For medium components marked "ad lib" or "ad libitum" in a score or otherwise.</skos:definition>
