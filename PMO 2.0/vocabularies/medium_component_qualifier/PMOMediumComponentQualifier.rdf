  <rdf:Description rdf:about="http://performedmusicontology.org/2.0/vocabularies/medium_component_qualifier/offstage">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">offstage</skos:prefLabel>
    <skos:definition xml:lang="en">For a  medium component that is marked to be performed or is performed offstage, often out of sight of the audience.</skos:definition>
    <rdfs:comment xml:lang="en">Value for: MediumComponentQualifier.</rdfs:comment>
    <skos:inScheme rdf:resource="http://performedmusicontology.org/2.0/vocabularies/medium_component_qualifier/"/>
    <skos:changeNote rdf:nodeID="n4ef25b41a79d444694ea6928ac67eb07b4"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://performedmusicontology.org/2.0/vocabularies/medium_component_qualifier/solo">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">solo</skos:prefLabel>
    <skos:definition xml:lang="en">For a medium component marked "solo" in a score or is clearly a solo part in the context of the work, such as the oboe part in a concerto for oboe and orchestra.</skos:definition>
    <rdfs:comment xml:lang="en">Value for: MediumComponentQualifier.</rdfs:comment>
    <skos:inScheme rdf:resource="http://performedmusicontology.org/2.0/vocabularies/medium_component_qualifier/"/>
    <skos:changeNote rdf:nodeID="n4ef25b41a79d444694ea6928ac67eb07b6"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://performedmusicontology.org/2.0/vocabularies/medium_component_qualifier/obbligato">
