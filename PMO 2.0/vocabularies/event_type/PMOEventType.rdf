  <rdf:Description rdf:about="http://performedmusicontology.org/2.0/vocabularies/event_type/">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
    <rdfs:label xml:lang="en">PMO Event Type Vocabulary</rdfs:label>
    <dc1:title xml:lang="en">PMO Event Type Vocabulary</dc1:title>
    <skos:definition xml:lang="en">The PMO Event Type Vocabulary is a controlled vocabulary that indicates the type of BIBFRAME Event being describe, e.g, Ceremony, Performance. It was developed for the Performed Music Ontology 2.0, as an extension to bf:Event, but may be used with other ontologies.</skos:definition>
  </rdf:Description>
