>
  <rdf:Description rdf:about="http://performedmusicontology.org/2.0/vocabularies/event_type/rehearsal">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Rehearsal</skos:prefLabel>
    <skos:definition xml:lang="en">Event in which a performer or group of performers prepare for a performance.</skos:definition>
    <rdfs:comment xml:lang="en">Use as type of bf:Event.</rdfs:comment>
    <skos:inScheme rdf:resource="http://performedmusicontology.org/2.0/vocabularies/event_type/"/>
    <skos:changeNote rdf:nodeID="na1cb2b217b574f14930288ba3be0d2dcb13"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://performedmusicontology.org/2.0/vocabularies/event_type/open_mic_performance">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Open mic performance</skos:prefLabel>
    <skos:definition xml:lang="en">Live performance at a coffee house, night club, or other venue by amateur performers.</skos:definition>
    <rdfs:comment xml:lang="en">Use as type of bf:Event.</rdfs:comment>
    <skos:inScheme rdf:resource="http://performedmusicontology.org/2.0/vocabularies/event_type/"/>
    <skos:changeNote rdf:nodeID="na1cb2b217b574f14930288ba3be0d2dcb10"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://performedmusicontology.org/2.0/vocabularies/event_type/master_class">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Master class</skos:prefLabel>
    <skos:definition xml:lang="en">Event in which a recognized performer critiques the performances of one or more other performers.</skos:definition>
    <rdfs:comment xml:lang="en">Use as type of bf:Event.</rdfs:comment>
    <skos:inScheme rdf:resource="http://performedmusicontology.org/2.0/vocabularies/event_type/"/>
    <skos:changeNote rdf:nodeID="na1cb2b217b574f14930288ba3be0d2dcb9"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="na1cb2b217b574f14930288ba3be0d2dcb3">
    <rdf:value>NEW</rdf:value>
    <dc1:date>2024-04-30</dc1:date>
  </rdf:Description>
  <rdf:Description rdf:about="http://performedmusicontology.org/2.0/vocabularies/event_type/concert">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Concert</skos:prefLabel>
    <skos:definition xml:lang="en">Live musical performance of one or more works in front of an audience or in a single recording session.</skos:definition>
    <rdfs:comment xml:lang="en">Use as type of bf:Event.</rdfs:comment>
    <skos:inScheme rdf:resource="http://performedmusicontology.org/2.0/vocabularies/event_type/"/>
    <skos:changeNote rdf:nodeID="na1cb2b217b574f14930288ba3be0d2dcb5"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://performedmusicontology.org/2.0/vocabularies/event_type/benefit_concert">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Benefit concert</skos:prefLabel>
    <skos:definition xml:lang="en">Concert held for charitable purposes, i.e., to raise funds or awareness.</skos:definition>
    <rdfs:comment xml:lang="en">Use as type of bf:Event.</rdfs:comment>
    <skos:inScheme rdf:resource="http://performedmusicontology.org/2.0/vocabularies/event_type/"/>
    <skos:changeNote rdf:nodeID="na1cb2b217b574f14930288ba3be0d2dcb2"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://performedmusicontology.org/2.0/vocabularies/event_type/recording_session">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Recording session</skos:prefLabel>
    <skos:definition xml:lang="en">Event in which one or more performances are recorded with the aim of producing a recording for issue.</skos:definition>
    <rdfs:comment xml:lang="en">Use as type of bf:Event.</rdfs:comment>
    <skos:inScheme rdf:resource="http://performedmusicontology.org/2.0/vocabularies/event_type/"/>
    <skos:changeNote rdf:nodeID="na1cb2b217b574f14930288ba3be0d2dcb12"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://performedmusicontology.org/2.0/vocabularies/event_type/commmand_performance">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Command performance</skos:prefLabel>
    <skos:definition xml:lang="en">Concert or performance done at the request of a monarch or other head of state.</skos:definition>
    <rdfs:comment xml:lang="en">Use as type of bf:Event.</rdfs:comment>
    <skos:inScheme rdf:resource="http://performedmusicontology.org/2.0/vocabularies/event_type/"/>
    <skos:changeNote rdf:nodeID="na1cb2b217b574f14930288ba3be0d2dcb4"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="na1cb2b217b574f14930288ba3be0d2dcb9">
    <rdf:value>NEW</rdf:value>
    <dc1:date>2024-04-30</dc1:date>
  </rdf:Description>
  <rdf:Description rdf:about="http://performedmusicontology.org/2.0/vocabularies/event_type/concert_tour">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Series of concerts by a performer or group of performers in different cities, countries, or locations.</skos:prefLabel>
    <skos:definition xml:lang="en">An ensemble of 7 performers.</skos:definition>
    <rdfs:comment xml:lang="en">Use as type of bf:Event.</rdfs:comment>
    <skos:inScheme rdf:resource="http://performedmusicontology.org/2.0/vocabularies/event_type/"/>
    <skos:changeNote rdf:nodeID="na1cb2b217b574f14930288ba3be0d2dcb7"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://performedmusicontology.org/2.0/vocabularies/event_type/first_performance">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">First performance</skos:prefLabel>
    <skos:definition xml:lang="en">The first known performance of a work.</skos:definition>
    <rdfs:comment xml:lang="en">Use as type of bf:Event.</rdfs:comment>
    <skos:inScheme rdf:resource="http://performedmusicontology.org/2.0/vocabularies/event_type/"/>
    <skos:changeNote rdf:nodeID="na1cb2b217b574f14930288ba3be0d2dcb8"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://performedmusicontology.org/2.0/vocabularies/event_type/ceremony">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Ceremony</skos:prefLabel>
    <skos:definition xml:lang="en">Ritual event, performed on special occasions (e.g., death, baptism, wedding, graduation, equinox, coronation, victory).</skos:definition>
    <rdfs:comment xml:lang="en">Use as type of bf:Event.</rdfs:comment>
    <skos:inScheme rdf:resource="http://performedmusicontology.org/2.0/vocabularies/event_type/"/>
    <skos:changeNote rdf:nodeID="na1cb2b217b574f14930288ba3be0d2dcb3"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://performedmusicontology.org/2.0/vocabularies/event_type/performance">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Performance</skos:prefLabel>
    <skos:definition xml:lang="en">Event in which a performer or group of performers perform an improvisation or pre-existing work to an audience or in a recording studio.</skos:definition>
    <rdfs:comment xml:lang="en">Use as type of bf:Event.</rdfs:comment>
    <skos:inScheme rdf:resource="http://performedmusicontology.org/2.0/vocabularies/event_type/"/>
    <skos:changeNote rdf:nodeID="na1cb2b217b574f14930288ba3be0d2dcb11"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://performedmusicontology.org/2.0/vocabularies/event_type/concert_series">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Concert series</skos:prefLabel>
    <skos:definition xml:lang="en">Group of concerts in the same geographic area by a single performer or brought together by a common theme, usually available for sale as a package. e.g., an orchestra concert season, a series featuring chamber ensembles.</skos:definition>
    <rdfs:comment xml:lang="en">Use as type of bf:Event.</rdfs:comment>
    <skos:inScheme rdf:resource="http://performedmusicontology.org/2.0/vocabularies/event_type/"/>
    <skos:changeNote rdf:nodeID="na1cb2b217b574f14930288ba3be0d2dcb6"/>
  </rdf:Description>
